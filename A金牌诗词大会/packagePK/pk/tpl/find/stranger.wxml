<template name="pk-find-friend">
    <view class="pk-find-friend">
        <view class="my-info">
            <image src="{{myInfo.headImgUrl}}"></image>
            <text>来自{{myInfo.province}}</text>
            <text>出场费：{{myInfo.appearanceFee}}</text>
        </view>

        <view class="enemy-info">
            <image src="{{enemyInfo.headImgUrl}}"></image>
            <text>来自{{enemyInfo.province}}</text>
            <text>出场费：{{enemyInfo.appearanceFee}}</text>
        </view>
    </view>
</template>
