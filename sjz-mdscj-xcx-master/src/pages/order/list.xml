<form bindsubmit="formSubmit" report-submit='true'>
  <button formType="submit" class="formIdBtn">
    <view>
      <component key="switchTab"></component>
      <component key="orders"></component>
      <view class="empty" wx:if="{{props.orders.length === 0}}">
        <view class="image-wrapper">
          <image src="../../images/empty@3x.png"></image>
        </view>
        <view class="tip">还没有相关订单哟~</view>
      </view>
    </view>
  </button>
</form>
