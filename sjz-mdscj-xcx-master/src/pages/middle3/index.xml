<form bindsubmit="formSubmit" report-submit='true'>
  <button formType="submit" class="formIdBtn">
    <view class="product-wrap">
      <view class="loading" wx:if="{{props.isRequesting}}">加载中...</view>
      <view class="old-products {{state.isScolled ? 'show' : ''}}" hidden="{{!state.isShowOld}}">
        <component key="oldProductsCom" />
      </view>
      <block>
        <list key="ProductCard"/>
        <!--<button open-type="contact" class="contact"></button>-->
      </block>
    </view>
  </button>
</form>
