}

.main text.t1::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  background: url('https://lingju360.com/miniapp/resources/image/icon/4.png') no-repeat;
  background-size: 30rpx 30rpx;
  position: absolute;
  left: 15rpx;
  top: 15rpx;
}

.main text.t2::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  background: url('https://lingju360.com/miniapp/resources/image/icon/1.png') no-repeat;
  background-size: 30rpx 30rpx;
  position: absolute;
  left: 15rpx;
  top: 15rpx;
}

.main .phoneRight {
  position: absolute;
  right: 0px;
