.g_alert{
  background: rgba(0,0,0,0.6);
  position: fixed;
  z-index: 99999;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-80%);
  text-align: center;
  border-radius: 20rpx;
}
.g_alert image{
  display: block;
  margin: 20rpx auto 0;
  width: 100rpx;
  height: 100rpx;
}
.g_alert .p{
  color: #fff;
  font-size: 28rpx;
  margin:10rpx 20rpx 20rpx;
  max-width: 200rpx;
  display: inline-block;
  word-wrap:break-word;
  overflow:hidden;
}