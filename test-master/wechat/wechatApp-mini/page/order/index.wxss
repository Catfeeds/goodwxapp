/* page/orderList/orderdetail/index.wxss */
.bac{
  background: #fff;
  padding: 30rpx;
  border-top: 20rpx solid #f0f0f0;
  border-bottom: 20rpx solid #f0f0f0;
}
.order_num{
  height:98rpx;
  line-height:98rpx;
  font-weight:bold;
  border-bottom: 1px dotted #f0f0f0;
  font-size:30rpx;
}
.order_num text{
  font-weight:800;
  font-size: 28rpx;
}
.order_c{
  position: relative;
    padding:35rpx 0 15rpx 0rpx;
    font-size:30rpx;
    
}
.order_c >view{
   float: left;
   width:90%;
   color:#333333;
}
 .order_c .order_c_d >view{
   margin-bottom: 20rpx;
   height: 40rpx;
   overflow: hidden;
 }
.order_c .order_c_d >view:nth-child(1){
     /* letter-spacing: 18rpx; */
}
.order_c .order_c_d >view:nth-child(1) text{
  letter-spacing: 0rpx;
  color:#E91C23;
}
.tishi{
  border-top: 1px double #f0f0f0;
  font-weight:bold;
  padding: 20rpx 0;
  font-size: 30rpx;
  color: #ff8502;
} 
.menu_d{
  border-bottom: 1px solid #f0f0f0;
}

.menu_d .menu_d_list{
  width:610rpx;
  margin-left:50rpx;
}
.menu_d .menu_d_list view:nth-child(1){
   height:78rpx;
   line-height: 78rpx;
   border-bottom: 1px solid #BBBBBB;
}
.menu_d .menu_d_list view:nth-child(1) text:nth-child(1){
  margin-right:200rpx;
}
.menu_d .menu_d_list view:nth-child(1) text:nth-child(2){
  margin-right:180rpx;
}
.menu_d_list_c{
  padding: 25rpx 0  40rpx 50rpx;
  width: 610rpx; 
  font-size:33rpx;
}
.menu_d_list_c>view{
  margin-bottom: 40rpx;
}
.menu_d_list_c .e_e >view{
  float:left;
}
.menu_d_list_c .pri{
  margin-left:100rpx;
  display: inline-block;
}
.menu_d_list_c .dishname{
  display: inline-block;
  width: 160rpx;
}
.menu_d_list_c .especial{
  margin-bottom: 0rpx;
  margin-left:50rpx;
  position:relative;
  top:-25rpx;
}
.dels,.adds{
  display: inline-block;
  height:40rpx;
  line-height:40rpx;
  text-align: center;
  width:40rpx;
  border:1px solid;
  border-radius:50% 50%; 
  color: #444444;
}
.menu_d_list_c .e_e .menu_d_list_deal{
  float: right;
}
.menu_d_list_deal text{
  padding:0 20rpx;
}
.ta_tl{
  padding: 30rpx 50rpx 80rpx 50rpx; 
}
.ta_tl view:nth-child(1){
  font-size:30rpx;
  text-align:right;
}
.ta_tl view:nth-child(1) text{
  color:#E91C23;
}
.ta_tl view:nth-child(2){
  font-size:30rpx;
  font-weight:bold;
  margin-top:50rpx;
}

.submit {
