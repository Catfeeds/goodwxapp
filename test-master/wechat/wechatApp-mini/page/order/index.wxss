}


.footer {
  position: fixed;
  bottom: -1rpx;
  height: 100rpx;
  width: 100%;
  left: 0;
  background: #fff;
  text-align: center;
  border-top: 1rpx solid #f0f0f0;
  z-index: 9999999999999999999;
}

.footer .rmb {
  line-height: 100rpx;
  padding-left: 110rpx;
  float: left;
  color: #ff3d5e;
}
.footer .sub_box{
  padding: 20rpx;
  top: 0px;
  display: inline-block;
  right: 0px;
}
.footer .sub {
  height: 60rpx;
  line-height: 60rpx;
  color: #fff;
  text-align: center;
  width: 200rpx;
  background-color: #ff8400;
  font-size:30rpx;
  border-radius: 8rpx;
  float: right;
}


.footer .num {
  position: absolute;
  background: #fff;
  width: 80rpx;
  height: 60rpx;
  left: 20rpx;
  top: 20rpx;
  z-index: 9999999999;
  text-align: center;
  line-height: 56rpx;
  font-size: 30rpx;
  color: #333333;
  box-sizing: border-box;
}
.table .li view text {
  width: 160rpx !important;
  /* height: 50rpx; */
  position: relative;
  /* top: -10rpx; */
  display: inline-block;
}

.qrCode{
  position: absolute;
  width: 200rpx !important;
  height: 200rpx !important;
  right:30rpx;
  top:35rpx;

}
.qrCode image{
  width: 100%;
  height: 100%;
}

._border{
  padding:0 0 0 30rpx;
  border: 1rpx solid #ebebeb;
  border-radius: 15rpx;
}