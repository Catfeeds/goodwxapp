<view class="selfCheckout" wx:if="{{contentlist.length}}">

  <view class='li' wx:for="{{contentlist}}" wx:key="{{index}}" catchtap='toOrder_2' data-deskId='{{item.deskId}}' data-id='{{item.id}}' data-orderNo='{{item.orderNo}}'>
  
    <view class='_border'>
    
      <view class='_t'>
        <view class='icon'></view>
        <view class='num'>{{item.deskNo}}</view>
        <view class='r_text'>
          <view>就餐人数：{{item.useNum}}人</view>
          <view>订单号：{{item.orderNo}}</view>
          
        </view>
      </view>

      <view class='_b'>
      
        <view>消费金额： <text>￥{{item.actualMoney}}</text> </view>
      
      </view>
    
    </view>
  
  </view>

</view>


<view class='selfCheckout_no' wx:if="{{!contentlist.length}}">


  <view class='i1'>
    <image src='http://lingju360.com/miniapp/resources/image/new/0.png'></image>
  </view>
  <view class='t1'>暂无订单</view>

</view>