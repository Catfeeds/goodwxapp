
<view class='suc_c' wx:if="{{GroupPurchase.length}}" wx:for="{{GroupPurchase}}" wx:key="{{index}}">
    <view class='over'>
       <view class='food_tag'>{{item.name}}</view><view class='reward'>{{item.num}}张</view>
    </view>    
    <view class='my_reward' >
        <view class='t1 {{active_index == item.str ? "active" :"" }}' wx:for="{{item.codeJson}}" wx:key="{{index}}">
           
           <view class='tit' data-index='{{item.str}}'  catchtap='change_active_index'>
            券码：<text>  {{item.str}}</text> <text >查看二维码</text>
           </view>

          <view class='img_box'>
            <image src='{{item.qrcode}}' ></image>
          </view>

        </view>
        
    </view>     
</view>

<view class='selfCheckout_no' wx:if="{{!GroupPurchase.length}}">
  <view class='i1'>
    <image src='http://lingju360.com/miniapp/resources/image/new/0.png'></image>
  </view>
  <view class='t1'>暂无数据</view>
</view>