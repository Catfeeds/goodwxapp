/* page/index2/index.wxss */
.big-view{
  overflow-x: hidden;
}
.selfCheckout_no {
margin-top:0rpx;
padding: 30rpx 0;
border-top: 1rpx solid #f2f2f2;
}

/*第一部分*/
.address-box {
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  padding-left: 40rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}

.address-box .head-icon {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  overflow: hidden;
  /* border: 1rpx solid #f0f0f0; */
}

.address-box .head-icon image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.address-box .food-and-address {
  padding-left: 60rpx;
}

.address-box .food-and-address .food-name {
  font-size: 32rpx;
  color: #333;
  margin-top: 19rpx;
}

.address-box .food-and-address .address {
  position: relative;
  margin-top: 6rpx;
}

.address-box .food-and-address .address .address-icon {
  position: absolute;
  top: 14rpx;
  display: inline-block;
  width: 17rpx;
  height: 26rpx;
  background: url('http://lingju360.com/miniapp/resources/image/index3/location@2x.png')no-repeat;
  background-size: 100% 100%;
}

.address-box .food-and-address .address .address-name {
  display: inline-block;
  position: relative;
  top: -4rpx;
  left: 35rpx;
  font-size: 22rpx;
  color: #666;
  width: 460rpx;
  word-wrap: break-word;
  line-height: 40rpx;
}

/*第二部分和第四部分公共样式*/

.order-food-box {
  padding-bottom: 23rpx;
}

.order-food-box .order-food-ul {
  display: flex;
  padding: 24rpx 30rpx 0;
  flex-direction: row;
  box-sizing: border-box;
  flex-wrap: wrap;
}

.order-food-box .order-food-ul .order-food-li {
  flex: 1;
  text-align: center;
  min-width: 33%;
  margin-bottom: 40rpx;
}


.order-food-box .order-food-ul .order-food-li .order-food-size {
  text-align: center;
}

.order-food-box .order-food-ul .order-food-li text {
  font-size:28rpx;
  color: #333;
}

.order-food-box .order-food-ul .order-food-li image {
  width: 97rpx;
  height: 97rpx;
}

.order-food-box .order-food-system {
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  padding-left: 45rpx;
}

.order-food-box .order-food-system .self-help-order,
.order-food-box .order-food-system .RichScan {
  position: relative;
  width: 318rpx;
  height: 116rpx;
  line-height: 116rpx;
  border-radius: 10rpx;
}

.order-food-box .order-food-system .self-help-order .order-food-system-size,
.order-food-box .order-food-system .RichScan .order-food-system-size {
  position: absolute;
  right: 41rpx;
  font-size: 32rpx;
  color: #fff;
}

.order-food-box .order-food-system .self-help-order {
  background-color: #fa6c76;
}

.order-food-box .order-food-system .self-help-order .self-help-order-icon {
  position: absolute;
  top: 50%;
  left: 49rpx;
  display: inline-block;
  width: 71rpx;
  height: 66rpx;
  margin-top: -33rpx;
  background: url('http://lingju360.com/miniapp/resources/image/index3/edit@2x.png')no-repeat;
  background-size: 100% 100%;
}

.order-food-box .order-food-system .RichScan {
  background-color: #fb9d38;
  margin-left: 23rpx;
}

.order-food-box .order-food-system .RichScan .RichScan-icon {
  position: absolute;
  top: 50%;
  left: 49rpx;
  display: inline-block;
  width: 65rpx;
  height: 65rpx;
  margin-top: -33rpx;
  background: url('http://lingju360.com/miniapp/resources/image/index3/scan@2x.png')no-repeat;
  background-size: 100% 100%;
}

/*第三部分*/

.discount-coupon-box {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.discount-coupon-box  .discount-coupon-title {
  text-align: center;
  position: relative;
}

.discount-coupon-box  .discount-coupon-title .discount-coupon-title-icon {
  display: inline-block;
  width: 29rpx;
  height: 27rpx;
  background: url('http://lingju360.com/miniapp/resources/image/index3/pinkGift@2x.png')no-repeat;
  background-size: 100% 100%;
}

.discount-coupon-box  .discount-coupon-title .discount-coupon-title-size {
  font-size: 32rpx;
  color: #333;
  margin-left: 18rpx;
}
.discount-coupon-box  .discount-coupon-title .more{
  float: right;
  color: #666;
  font-size: 28rpx;
  padding-right: 40rpx;
  margin-top: 10rpx;
  margin-right: 30rpx;
  position: absolute;
  right: 0;
  background: url(http://lingju360.com/miniapp/resources/image/manage/index/icon.png@2x.png) no-repeat right center;
  background-size: 26rpx 26rpx;
}
.discount-coupon-box .discount-coupon-ul {
  padding-top: 20rpx;
}

.discount-coupon-box .discount-coupon-ul .discount-coupon-li {
  width: 690rpx;
  height: 182rpx;
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  border: 1rpx solid #fed4d8;
  margin: 20rpx auto 0 auto;
}

.discount-coupon-box .discount-coupon-ul .discount-coupon-li .money-limit {
  width: 165rpx;
  margin-left: 32rpx;
  margin-top: 42rpx;
  text-align: left;
}

.discount-coupon-box .discount-coupon-ul .discount-coupon-li .money-limit .money {
  font-size: 60rpx;
  color: #fc2a3e;
}

.discount-coupon-box .discount-coupon-ul .discount-coupon-li .money-limit .full-money {
  font-size: 22rpx;
  color: #fc2a3e;
  text-indent: 20rpx;
}

.discount-coupon-box .discount-coupon-ul .discount-coupon-li .discount-coupon-data {
  margin-left: 18rpx;
}

.discount-coupon-box .discount-coupon-ul .discount-coupon-li .discount-coupon-data .ticket-size {
  font-size: 28rpx;
  color: #fc2a3e;
  margin-top: 30rpx;
}

.discount-coupon-box .discount-coupon-ul .discount-coupon-li .discount-coupon-data .limited-stores {
  font-size: 22rpx;
  color: #666;
  margin-top: 10rpx;
}

.discount-coupon-box .discount-coupon-ul .discount-coupon-li .discount-coupon-data .validity-data {
  font-size: 22rpx;
  color: #666;
  margin-top: 10rpx;
  width: 245rpx;
}



.discount-coupon-box .discount-coupon-ul .discount-coupon-li .discount-coupon-type .receive-ticket {
  width: 200rpx;
  height: 60rpx;
  margin-top: 60rpx;
}
.already-received-ticket{
  width: 130rpx;
  height: 130rpx;
  margin-top: 25rpx;
  margin-left: 50rpx;
}
/*单独第四部分样式*/
.buy-stamps-box .discount-coupon-title .discount-coupon-title-icon{
display:inline-block;
width:35rpx;
height:28rpx;
background:url('http://lingju360.com/miniapp/resources/image/index3/coupon@2x.png')no-repeat;
background-size:100% 100%;
}
.buy-stamps-box .discount-coupon-ul .discount-coupon-li{
  border:1rpx solid #ffe7cc;
}
.buy-stamps-box .discount-coupon-ul .discount-coupon-li .money-limit .money {
  color: #ff8502;
}
.buy-stamps-box .discount-coupon-ul .discount-coupon-li .money-limit .full-money{
  color: #ff8502;
}
.buy-stamps-box .discount-coupon-ul .discount-coupon-li .discount-coupon-data .limited-stores{
  color: #ff8502;
  font-size: 28rpx;
  margin-top: 75rpx;
}

/*分割线*/

.split-line {
  width: 100%;
  height: 20rpx;
  background-color: #f2f2f2;
}
/*奖励弹框  */
.aword{
  width:634rpx;
  height: 528rpx;
  background: red;
  position:fixed;
  z-index:1500;
  top:300rpx;
  left:60rpx;
  background: url(http://lingju360.com/miniapp/resources/image/aword1.png) center no-repeat; 
  background-size:634rpx 528rpx;
}
.aword_2{
  background: url(http://lingju360.com/miniapp/resources/image/aword3.png) center no-repeat; 
  background-size:634rpx 528rpx;
  margin-top:0px;
}
.aword_2 >view:nth-child(2){
  width: 100%;
}
.aword_2 >view:nth-child(2) .getmine{
   height: 130rpx;
   width:100%;
   text-align: center;
   line-height: 130rpx;
   color:#fff;
}
.aword_2 >view:nth-child(2) .scores{
   height: 130rpx;
   width:100%;
   text-align: center;
   line-height: 130rpx;
   color:#666666;
   font-size:30rpx;
}
.aword_2 >view:nth-child(2) .shareit{
  font-size:30rpx;
  width:70%;
  margin:0 auto;
}
.aword_2 >view:nth-child(2) .shareit >view{
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  width:200rpx;
  float: left;
  border: 1px solid;
  border-radius: 8rpx 8rpx;
}
.aword_2 >view:nth-child(2) .shareit .shares{
  color:#FC5867;
  height:65rpx;
  line-height:65rpx;
  text-align:center;
  width:220rpx;
  float:left;
  border:1px solid;
  border-radius:8rpx 8rpx;
  font-size:30rpx;

}
.aword_2 >view:nth-child(2) .shareit .seemine{
  float: right;
  color:#FF9320;

}
.aword >view{
  margin:0 auto;
  width: 57%;
}
.aword >image{
  width:31rpx;
  height:31rpx;
  top:0;
  right: 0;
  position:absolute;
}
.aword  .aword2{
  width:63%;
  text-align: center;
  margin-top:70rpx;
  color:#fff;
}
.aword >view:nth-child(2) .happy{
  font-size:35rpx;
  margin-bottom:30rpx;
}
.aword >view:nth-child(2) .lastdate{
  font-size:26rpx;
  margin-bottom:36rpx;
}
.aword >view:nth-child(2) .getit{
  color:#fc2a3e;
  background: #fff;
  height:60rpx;
  line-height:60rpx;
  width:50%;
  margin: 0 auto;
  border-radius: 80rpx 80rpx;
}
.aword >view:nth-child(1){
  color: #fc2a3e; 
  padding-top:65rpx;
  font-size:26rpx;
}
.aword >view:nth-child(1) .m_m text:nth-child(1){
   font-size:26rpx;   
}
.aword >view:nth-child(1) .m_m text:nth-child(2){
   font-size:50rpx;   
}
.aword >view:nth-child(1) .m_m_shop text{
   margin-left:10rpx;
   font-size:26rpx;
}
.aword >view:nth-child(1) .m_m_shop text:nth-child(1){
  font-size:35rpx;
}
.aword >view:nth-child(1) .m_m, .aword >view:nth-child(1) .m_m_shop{
  display:inline-block;
}
.aword >view .required{
  text-align:right;
  width:290rpx;
}