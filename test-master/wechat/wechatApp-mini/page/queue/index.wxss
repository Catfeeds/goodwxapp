.queue{
  padding:0rpx 60rpx 30rpx;
  display: block;
}
.queue .text{
  text-align: center;
  margin-top: 60rpx;
  font-size: 36rpx;
  font-weight: 800;
  color: #333;
  margin-bottom: 30rpx;
  overflow: hidden;
}
.queue scroll-view{
  height: 940rpx;
  margin-bottom: 20rpx;
}
.queue scroll-view .li{
  float: left;
  width: 33.333333333%;
  text-align: center;
  padding: 20rpx 0;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}



.queue scroll-view .li view{
  display: inline-block;
  width: 150rpx;
  height: 150rpx;
  background-color: #e5e5e5;
  border-radius:50%;
  line-height: 150rpx; 
  color: #000;
  font-size: 45rpx;
  transition: all ease 0.1s;
}

.queue scroll-view .li.active view{
  background-color: #ff8400;
  color: #fff;
}
.queue .t{
  position: fixed;
  width: 80%;
  margin:0 auto;
  bottom: 150rpx;
  text-align: center;
  letter-spacing:1rpx;
  line-height: 48rpx;
  color: #666;
  font-size: 28rpx;
}

.btn {
  position: fixed;
  bottom: 40rpx;
  background:#ff8400;
  color:#fff;
  border-radius:60rpx;
  margin:0 auto;
  height:90rpx;
  text-align:center;
  line-height:90rpx;
  width: 80%;
  left: 10%;
  font-size: 32rpx;
  padding: 0;
}
