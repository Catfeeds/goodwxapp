<view class="coupon">

  <view class='li' wx:for="{{CashCouponInfo}}" wx:key="{{index}}">
  
    <view class='box_left'>
    
      <view class='i_left'>
        <view class='num'> <text>￥</text>{{item.money}}</view>
        <view class='t2'>{{item.isShopLimit == false ? "不限使用门店":"部分门店使用"}}</view>
      </view>

      <view class='i_right'>
        <view class='t1'>{{item.name}}</view>
        <view class='t2'>限会员微信端使用</view>
        <view class='t3'>有效期至： {{item.endTime}}</view>
      </view>
    
    </view>
  
    <view class='box_right'>
    
        <view class='t1'>售价 <text>{{item.price}}</text> <text >元</text> </view>
        <view class='t2' data-id="{{item.id}}" catchtap='buyVoucher'>购买</view>

    </view>

  </view>

</view>
