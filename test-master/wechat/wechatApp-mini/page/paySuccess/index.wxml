
<view class="success">


<view class='_border'>

  <view class="top">
  <image src="https://lingju360.com/miniapp/resources/image/gg.png"></image>
  <view>支付成功</view>
</view>


<view class='text_list'>

  <view class='li'>
    <text>订单编号：</text>
    <text>{{queryPayResult.orderNo}}</text>
  </view>

  <view class='li'>
    <text>支付金额：</text>
    <text class='money'>￥{{queryPayResult.actualAmount ? queryPayResult.actualAmount :queryPayResult.actualMoney }}</text>
  </view>

</view>


<view class='success_btn'>
  
    <view class='li'>
      <navigator open-type="switchTab" url='/page/index/index' hover-class='none'>
      <view class='btn'>返回首页</view>
      </navigator>
    </view>


    <view class='li' wx:if="{{MoneyCoupon}}">
      <navigator  open-type="redirectTo" url='/page/coupon/couponList/index?cardType=1' hover-class='none'>
        <view class='btn' catchtap=''>查看代金券</view>
      </navigator>
    </view>
  
  </view>

</view>



<view class="menu_list" wx:if="{{0}}">

  <view class="li">
    <text class="t1">桌号：</text>
    <text class="t2">{{queryPayResult.deskNo}}</text>
  </view>

  <view class="li">
    <text class="t1">订单号：</text>
    <text class="t2">{{queryPayResult.orderNo}}</text>
  </view>

  <view class="li">
    <text class="t1">支付方式：</text>
    <text class="t2">{{queryPayResult.payTypeStr}}</text>
  </view>

  <view class="li">
    <text class="t1">下单时间：</text>
    <text class="t2">{{queryPayResult.createdDate}}</text>
  </view>

  <view class="li">
    <text class="t1">支付金额：</text>
    <text class="t2">￥{{queryPayResult.actualMoney}}</text>
  </view>

</view>

