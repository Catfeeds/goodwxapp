 <view class="header">
  <image class="imgsss"  src="https://lingju360.com/miniapp/resources/image/index/banner1.png"></image>
</view> 

 <view class="main">
  <text class="t1">营业时间：{{startTime}}-{{endTime}}</text>
  <text class="t2">{{address}}</text>
  <view bindtap="phone" class="phoneRight">
    <text class="icon2_phone"></text>
  </view>
</view> 

<view class="twoSection">
  
  <view class="section">
    <input class="i1" placeholder="请输入姓名" bindinput="inputName" value="{{inputName}}" maxlength='10'/>
  </view>

  <view class="section">
    <input class="i2"  placeholder="请输入电话" bindinput="inputPhone" value="{{inputPhone}}" maxlength='11'/>
  </view>

<view class="section">
  <picker mode="time" value="{{time}}" start="{{startTime}}" end="{{endTime}}" bindchange="bindTimeChange">
    <view class="picker p4">
      时间: {{time}}
    </view>
  </picker>
</view>


<view class="section">
  
  <picker mode="date" value="{{date}}" start="{{date2}}" end="" bindchange="bindDateChange">
    <view class="picker p5">
      日期: {{date}}
    </view>
  </picker>
</view>



<text class="ts">姓名和电话是必填选项,请务必填写,谢谢配合!</text>

</view>
<form report-submit class="queue" bindsubmit="getdata">
<button formType="submit" class="btn">立即预约</button>
</form>