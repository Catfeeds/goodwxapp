/* page/dineManagement/index.wxss */
.dineManagement{
  padding: 30rpx;
  padding-bottom: 0;
  border-top: 20rpx solid #f0f0f0;
  border-bottom: 20rpx solid #f0f0f0;
}
.dineManagement .li{
  margin-bottom: 30rpx;
}
._border{
  padding: 30rpx;
  border: 1rpx solid #ebebeb;
  border-radius: 15rpx;
}

.dineManagement .li ._t{
  
  overflow: hidden;
  padding-top: 30rpx;
  position: relative;
}
.dineManagement .li ._t .b{
  position: absolute;
  bottom: 0;
  right: 0;
  background-color: #ff8502;
  color: #fff;
  font-size: 30rpx;
  padding: 10rpx 30rpx;
  border-radius: 50rpx;
}

.dineManagement .li ._t .r_text{
  font-size: 28rpx;
  float: left;
  line-height: 60rpx;
}
.dineManagement .li ._b{
  text-align: left;
  font-size: 28rpx;
  
  padding-bottom: 30rpx;
  border-bottom: 1rpx dashed #e2e2e2;
}
.dineManagement .li ._b text{
  float: right;
  color: #fc2a3e;
}
.dineManagement_no{
  text-align: center;
  margin-top:280rpx;

}

.dineManagement_no image{
  width:231rpx;
  height:138rpx;

}

.dineManagement_no .t1{
  font-size:26rpx;
  margin:30rpx 0;
  color:#666;
}

.yell{
  color: #ff8502;
}