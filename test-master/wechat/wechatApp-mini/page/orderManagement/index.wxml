<view wx:if="{{!userphone}}" catchtap="toLogin" class="toLogin"></view> 
<view class="orderManagement">

  <view class='li'>
    <navigator url='/page/orderManagement/dineManagement/index' hover-class='none'>
      <view class='_border'>
        <view class='img_box icon3'><image src='http://lingju360.com/miniapp/resources/image/orderManagement/icon3.png'></image></view>
        <text class='t1'>堂食订单</text>
        <text class='t2'>还有{{data.order}}笔待处理</text>
        <view class='icon'></view>
      </view>
    </navigator>
  </view>

  <view class='li'>
    <navigator url='/page/orderManagement/vouchersManagement/index' hover-class='none'>
      <view class='_border'>
        <view class='img_box icon2'><image src='http://lingju360.com/miniapp/resources/image/orderManagement/icon2.png'></image></view>
        <text class='t1'>代金券</text>
        <text class='t2'>还有{{data.cash}}笔待处理</text>
        <view class='icon'></view>
      </view>
    </navigator>
  </view>

  <view class='li' wx:if="{{0}}">
    <navigator url='/page/orderManagement/groupBuyManagement/index' hover-class='none'>
      <view class='_border'>
        <view class='img_box  icon4'><image src='http://lingju360.com/miniapp/resources/image/orderManagement/icon4.png'></image></view>
        <text class='t1'>团购</text>
        <text class='t2'>还有{{data.pack}}笔待处理</text>
        <view class='icon'></view>
      </view>
    </navigator>
  </view>

  <view class='li' wx:if="{{1}}">
    <navigator url='/page/orderManagement/takeawayBuyManagement/index' hover-class='none'>
      <view class='_border'>
        <view class='img_box  icon4'><image src='http://lingju360.com/miniapp/resources/image/orderManagement/icon1.png'></image></view>
        <text class='t1'>外卖</text>
        <!-- <text class='t2'>还有{{0}}笔待处理</text> -->
        <view class='icon'></view>
      </view>
    </navigator>
  </view>

  <view class='li'>
    <navigator url='/page/orderList/index' hover-class='none'>
      <view class='_border'>
        <view class='img_box icon1'><image src='http://lingju360.com/miniapp/resources/image/orderManagement/icon4.png'></image></view>
        <text class='t1'>预点菜</text>
        <!-- <text class='t2'>还有3笔待处理</text> -->
        <view class='icon'></view>
      </view>
    </navigator>
  </view>

</view>
