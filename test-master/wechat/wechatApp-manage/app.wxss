  box-sizing: border-box;
  z-index: 99999999;
  border-bottom: 20rpx solid #f0f0f0;

}

.header input {
  height: 60rpx;
  margin:0rpx auto;
  margin-top: 20rpx;
  border: 1rpx solid #ddd;
  border-radius: 80rpx;
  font-size: 28rpx;
  color: #666;
