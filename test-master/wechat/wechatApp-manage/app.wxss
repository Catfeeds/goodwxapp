/* page/vichily/index.wxss */

.header {
  overflow: hidden;
}

.imgsss {
  width: 100%;
  display: block;
}

.main {
  padding-top: 15rpx;
  height: 130rpx;
  border-top: 20rpx solid #f0f0f0;
  border-bottom: 20rpx solid #f0f0f0;
  position: relative;
  background: #fff;
}

.main text {
  display: block;
  padding-left: 60rpx;
  line-height: 60rpx;
  position: relative;
  color: #333;
  font-size: 28rpx;
  overflow: hidden;
  height: 60rpx;
  max-width: 500rpx;
}

.main text.t1::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  background: url('https://lingju360.com/miniapp/resources/image/icon/4.png') no-repeat;
  background-size: 30rpx 30rpx;
  position: absolute;
  left: 15rpx;
  top: 15rpx;
}

.main text.t2::after {
  content: '';
  width: 30rpx;
  height: 30rpx;
  background: url('https://lingju360.com/miniapp/resources/image/icon/1.png') no-repeat;
  background-size: 30rpx 30rpx;
  position: absolute;
  left: 15rpx;
  top: 15rpx;
}

.main .phoneRight {
  position: absolute;
  right: 0px;
  top: 0px;
  border-left: 1px solid #f0f0f0;
  height: 150rpx;
  width: 170rpx;
  text-align: center;
}

.main .phoneRight .icon2_phone {
  display: inline-block;
  margin-top: 40rpx;
  padding: 0px;
  height: 56rpx;
  width: 56rpx;
  background: url('https://lingju360.com/miniapp/resources/image/icon/p.png') no-repeat;
  background-size: 56rpx 56rpx;
}

.twoSection .title {
  text-align: center;
  border-bottom: 1px solid #ffc834;
  padding: 15rpx 0;
  color: #ffc834;
  font-size: 40rpx;
}

.twoSection .lj_firstDiv {
  margin: 60rpx 60rpx 20rpx;
  border: 1px solid #f0f0f0;
}

.twoSection .lj_firstDiv .table-title {
  padding: 20rpx;
  font-size: 30rpx;
  border-bottom: 1px solid #f0f0f0;
}

.twoSection .lj_firstDiv .table-title text {
  color: #ffc834;
}

.twoSection .lj_firstDiv .li-3 {
  text-align: center;
  overflow: hidden;
}

.twoSection .lj_firstDiv .li-3 .li {
  float: left;
  width: 33%;
  font-size: 40rpx;
}

.twoSection .lj_firstDiv .li-3 .li .t {
  display: inline-block;
  font-size: 35rpx;
  margin: 20rpx 0 0;
}

.twoSection .lj_firstDiv .li-3 .li .more {
  padding: 20rpx 0;
  font-size: 30rpx;
  color: #8e8e8e;
}

.twoSection .lj_firstDiv .li-3 .li .more text {
  color: #ffc834;
  font-size: 40rpx;
  margin-right: 10rpx;
}

.twoSection .ts {
  display: inline-block;
  padding: 0 60rpx;
  color: #8e8e8e;
  margin: 30rpx 0 30rpx;
  position: relative;
}

.twoSection .ts::after {
  content: '';
  position: absolute;
  left: 10px;
  top: 7rpx;
  padding: 0px;
  height: 30rpx;
  width: 30rpx;
  background: url('https://lingju360.com/miniapp/resources/image/icon/7.png') no-repeat;
  background-size: 30rpx 30rpx;
}

.btn {
  background: #ff8400;
