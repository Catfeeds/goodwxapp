  margin: 20rpx 0 40rpx;
}

.btn_bom {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 124rpx;
  border-top: 1rpx solid #c8c8c8;
  width: 100%;
  text-align: center;
  padding: 26rpx;
  box-sizing: border-box;
  background: #fff;
}

.btn_bom view {
  background-color: #ff8400;
  height: 72rpx;
  color: #fff;
  font-size: 30rpx;
  border-radius: 8rpx;
  line-height: 72rpx;
}

.table {
  background: #fff;
  margin-top: 30rpx;
  margin-bottom: 100rpx;
}

.table .hander, .table .body {
  color: #999;
  text-align: center;
}

.table  text {
  line-height: 90rpx;
  display: inline-block;
}

.table .li text:nth-of-type(1) {
  width: 175rpx;
}

.table .li text:nth-of-type(2) {
  width: 150rpx;
}

.table .li text:nth-of-type(3) {
  width: 150rpx;
}

.table .li text:nth-of-type(4) {
  width: 250rpx;
}

.table .body {
  color: #333;
  border-top: 1rpx solid #f0f0f0;
}

.table .body.li text:nth-of-type(1) {
  text-align: center;
}

.table .body.li text:nth-of-type(2) {
  color: #999;
}

.table .li {
  height: 90rpx;
  overflow: hidden;
}

.table .li view {
  width: 250rpx;
  display: inline-block;
  text-align: center;
  line-height: 90rpx;
  position: relative;
  top: 11rpx;
}

.table .li view image {
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
}

.table .li view text {
  width: 60rpx !important;
  height: 50rpx;
  position: relative;
  top: -10rpx;
  display: inline-block;
}

.table .foot {
  border-top: 1rpx solid #f0f0f0;
  color: #666;
  font-size: 30rpx;
  text-align: right;
  padding-right: 70rpx;
}

.table .foot text {
  color: #ff3d5e;
}

