page {
  background-color: #f0f0f0;
}

.tableManagement .table-management-list {
  display: flex;
  position: fixed;
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  box-shadow: 0 4rpx 20rpx rgba(0,0,0,0.3);
  background-color: #fff;
  z-index: 999;
}

.tableManagement .table-management-list view {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  color: #333;
  border-bottom: 6rpx solid #fff;
  transition: all 0.3s ease;
}

.tableManagement .table-management-list view.active {
  border-bottom: 6rpx solid #5fbaf4;
  color: #000;
}

.table-management-box {
  padding: 130rpx 0rpx 230rpx;
}
.table-management-box.show{
  padding-bottom: 20rpx;
}

.table-management-ul {
  overflow: hidden;
  font-size: 0;
  padding-left: 30rpx;
  padding-right: 10rpx;
}

.table-management-box .li {
  width: 33.33333333%;
  display: inline-block;
  font-size: 32rpx;
  padding: 0rpx 20rpx 20rpx 0;
  box-sizing: border-box;
}

.table-management-box .li .box {
  height: 210rpx;
  text-align: center;
  position: relative;
}

.table-management-box .li .box.free {
  background-color: #1aad19 ;
  border-radius: 14rpx;
}

.table-management-box .li .box.back0{
  background-color: #fff;
}
.table-management-box .li .box.back4{
  background-color: #b5b3b7;
}
.table-management-box .li .box.back1{
  background-color: #40dd6a;
}
.table-management-box .li .box.back2{
  background-color: #5ebbf5;
}
.table-management-box .li .box.back3{
  background-color: #b5b3b7;
}
.table-management-bottom {
  bottom: -250rpx;
  height: 230rpx;
  position: fixed;
  left: 0;
  width: 100%;
  background: #fff;
  box-shadow: 0 -4rpx 20rpx rgba(0,0,0,0.3);
  box-sizing: border-box;
  padding-top: 30rpx;
  transition: bottom 0.3s ease;
  
}
.table-management-bottom.show{
  bottom: 0;
}

.table-management-ul .li {
  width: 33.33333333%;
  display: inline-block;
  font-size: 32rpx;
  padding: 0rpx 20rpx 30rpx 0;
  box-sizing: border-box;
}
.table-management-box .li{
  padding: 0rpx 20rpx 20rpx 0;
  overflow: hidden;
}

.table-management-ul .li .box.active{
  background: #ff5a10 !important;
  color: #fff;
}

.table-management-box .li .box .num{
  padding-top: 100rpx;
  color: #333;
  font-size: 28rpx;
}
.table-management-box .li .box .t{
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  color: #333;
  font-size: 28rpx;
}
.table-management-box .li .box.active view{
  color: #fff;
}

.table-management-bottom .li .box, .box1 .li .box {
  border-radius: 8rpx;
  border: 1rpx solid #5fbaf4;
  padding: 14rpx;
  text-align: center;
  color: #5fbaf4;
  font-size: 28rpx;
  font-weight: 800;
}

.table-management-bottom .li .box.active, .box1 .li .box.active {
  background-color: #5fbaf4 !important;
  color: #fff;
}

.table-management-bottom .li .box.disabled, .box1 .li .box.disabled {
  background-color: #a1a1a1;
  color: #fff;
  border: 1rpx solid #a1a1a1;
}

.box1 {
  position: fixed;
  left: 0;
  width: 100%;
  height: 360rpx;
  background: #fff;
  opacity: 0;
  bottom: -390rpx;
  transition: all ease 0.3s;
}

.box1.show {
  bottom: 0;
  z-index: 9999999999;
  opacity: 1;
}

.box1 .table-management-ul {
  position: absolute;
  bottom: 0;
  width: 100%;
  box-sizing: border-box;
  text-align: right;
}

.box1 .title {
  background: #f0f0f0;
  text-align: center;
  padding:25rpx 20rpx;
  font-size: 30rpx;
  color: #333;
}

.content .li {
  position: relative;
  overflow: hidden;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #f0f0f0;
}

.content .li .right_btn {
  overflow: hidden;
  float: right;
}

.content .li .right_btn .imgbox {
  padding: 30rpx 10rpx;
  float: left;
}

.content .li .right_btn .num, .content .li .text {
  padding: 30rpx 10rpx;
  float: left;
  font-size: 28rpx;
  height: 42rpx;
  line-height: 42rpx;
}

.content .li .right_btn image {
  width: 42rpx;
  height: 42rpx;
}

.box6 .content .li .right_btn view {
  float: left;
  margin: 25rpx 10rpx;
  font-size: 28rpx;
  height: 42rpx;
  line-height: 42rpx;
  padding: 7.5rpx 30rpx;
  border-radius: 8rpx;
  border: 1rpx solid #fff;
}

.box6 .content .li .right_btn view.active {
  color: #5fbaf4;
  border: 1rpx solid #5fbaf4;
}

.center .table-management-ul .li {
  width: 50%;
}
.content .li .left_btn {
  padding-top: 21rpx;
  float: left;
  margin-left: 30rpx;
}
.content .li .left_btn input{
  border: 1rpx solid #ddd;
  padding: 5rpx 20rpx;
  font-size: 30rpx;
  color: #333;
  border-radius: 8rpx;
  width: 400rpx;
}
.content .li{
  height: 115rpx;
}

.content .li>image{
  position: absolute;
  top: 20rpx;
  right: 30rpx;
  width: 55rpx;
  height: 63rpx;
}