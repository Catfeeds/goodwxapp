<view class="user">
    <view class="block">
        <view class="li picture">
            <text>头像</text>
            <view><image src="{{userInfo.avatar}}" /></view>
        </view>
        <view class="li">
            <text>昵称</text>
            <text>{{userInfo.realname}}</text>
        </view>
        <view class="li">
            <text>手机号</text>
            <text>{{userInfo.phone}}</text>
        </view>
    </view>
    <view class="block">
        <view class="li">
            <text>密码</text>
            <text>*********</text>
        </view>
    </view>
     <view class="btn" catchtap='signout'>退出登录</view> 
</view>