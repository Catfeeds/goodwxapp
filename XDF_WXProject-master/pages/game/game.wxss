.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}

.right{
  color:green;
}
.error{
  color:red;
}
.gray{
  color:gray!important;
}
.keyword{
  display: inline-block;
  line-height:20vw;
  font-size:20vw;
  text-transform: uppercase;
}
.tip{
  font-size: 14px;
}
.tip-wrapper{
  padding: 6px 28px;
  background-color: #e1f7fd;
}
.words-wrapper{
  text-align: center
}
.v-wrapper{

  background-image:url("data:image/png;base64,") ;
