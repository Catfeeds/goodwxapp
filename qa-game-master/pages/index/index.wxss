@import "../../assets/stylesheets/base.wxss";
@import "../../assets/stylesheets/common.wxss";
page{
	background:#222d42;
}
.container {
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/bg.jpg) no-repeat center -140rpx;
	background-size: 750rpx 1333rpx;
	position:relative;
	width:100%;
	font-size:32rpx;
}
.banner{
	background:url(http://7xq41n.com1.z0.glb.clouddn.com/index_ban.png) no-repeat center;
	background-size: 730rpx 560rpx;
	width: 100%;
	height:560rpx;
	margin-top: 30rpx;
	position: relative;
}
.getRules{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/getRules.png) no-repeat center;
	background-size: 258rpx 73rpx;
	width: 258rpx;
	height:73rpx;
	position: absolute;
	top:13rpx;
	left:38rpx;
}

.totalChallenge{
	position: absolute;
	text-align: center;
	bottom: 15%;
  left: 50%;
  transform: translate(-50%, 0%);
	width: 750rpx;
}

.totalChallenge .text1{
	font-size: 24rpx;
	color: #ffffff;
	text-shadow: 2px 2px 2px #000000;
}

.totalChallenge .text3{
	font-size: 24rpx;
	color: #ffffff;
	text-shadow: 2px 2px 2px #000000;
}

.totalChallenge .text2{
	font-size: 32rpx;
	color: red;
	text-shadow: 2px 2px 2px #000000;
}


.beginBtn{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/btna1.png) no-repeat center;
	background-size: 413rpx 275rpx;
	width: 413rpx;
	height:275rpx;
	position: absolute;
	bottom: -205rpx;
	left:50%;
	transform:translate(-50%,0);
	-webkit-transform:translate(-50%,0);
}
.fixGzh{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/top.png) no-repeat center;
	background-size: 258rpx 106rpx;
	width: 258rpx;
	height:106rpx;
	position: fixed;
	top:55rpx;
	right:0;
}
.fixGzhBtn{
	width: 100%;
	height:100%;
	opacity: 0;
}
.home_tips{
	min-width: 522rpx;
	margin:240rpx auto 0;
	text-align: center;
	height: 60rpx;
	line-height: 60rpx;
	background-color: rgba(255,255,255,0.3);
	border-radius: 29rpx;
	font-size: 27rpx;
	color: #ffffff;
}
.content{
	padding: 0 0 45rpx 0;
}
.content .navBar{
	height:83rpx;
	text-align: center;
	margin: 44rpx 0 27rpx;
}
.content .navBar .nav{
	width: 221rpx;
	height:83rpx;
	display: inline-block;
	margin-right: 20rpx;
}
.content .navBar .last{
	margin-right: 0;
}
.content .navBar .nav01{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/btn_zl.png) no-repeat center;
	background-size: 221rpx 83rpx;
}
.content .navBar .nav02{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/btn_qn.png) no-repeat center;
	background-size: 221rpx 83rpx;
}
.content .navBar .nav03{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/btn_pf.png) no-repeat center;
	background-size: 221rpx 83rpx;
}
.content .navBar .nav01.active{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/btn_zl_on.png) no-repeat center;
	background-size: 221rpx 83rpx;
}
.content .navBar .nav02.active{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/btn_qn_on.png) no-repeat center;
	background-size: 221rpx 83rpx;
}
.content .navBar .nav03.active{
	background:url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/bnt_pf_on.png) no-repeat center;
	background-size: 221rpx 83rpx;
}
.content .part01{
	width: 646rpx;
	max-height: 834rpx;
	background-color: #dce8ff;
	border-radius: 3rpx;
	margin:0 auto;
	padding:0 25rpx 20rpx;
	overflow-y:scroll;
}
.content .part01 .pt{
	padding:40rpx 30rpx;
	text-align: center;
	font-size: 41rpx;
	color: #43609a;
	border-bottom: 2rpx solid #9dafd2;
	font-weight: bold;
}
.content .part01 .listMenu .listItem{
	display: flex;
	align-items: center;
	padding: 40rpx 30rpx 40rpx 5rpx;
	font-size: 23rpx;
	color: #666666;
	border-bottom: 2rpx solid #9dafd2;
}
.content .part01 .listMenu .last{
	border-bottom: none;
}
.content .part01 .listMenu .listItem .num{
	width: 46rpx;
	height: 46rpx;
	line-height:46rpx;
	background: #aaaaaa;
	border-radius: 100%;
	display: inline-block;
	text-align: center;
	color:#fff;
	margin-right: 10rpx;
}
.content .part01 .numOne{
	background: #ffa200!important;
}
.content .part01 .numTwo{
	background: #7b8db6!important;
}
.content .part01 .numThree{
	background: #de722b!important;
}
.content .part01 .face{
	width: 66rpx;
	height: 66rpx;
	margin-right: 10rpx;
	border-radius:100%;
}
.content .part01 .name{

}
.content .part01 .itemDetail{
    margin-left: auto;
    text-align: right;
}
.content .part01 .fontType01{
	font-size: 30rpx;
	color: #ff6600;
}
.content .part01 .fontType02{
	font-size: 23rpx;
	color: #ff4302;
}

.content .part02{
	width: 696rpx;
	background-color: #dce8ff;
	border-radius: 3rpx;
	text-align: center;
	margin:0 auto;
	padding: 73rpx 0 48rpx;
}
.content .part02 .logo{
	display: inline-block;
	width:312rpx;
	height: 346rpx;
}
.content .part02 .info{
	margin-top: 40rpx;
	font-size: 24rpx;
	color: #666666;
}
.content .part02 .btnShare{
	margin-top: 40rpx;
	width: 413rpx;
	height: 80rpx;
	line-height: 80rpx;
	background:#426098 url(https://staticdaily.zhongan.com/website/open/assets/wp/qaGame/wx.png) no-repeat 42rpx center;
	background-size: 51rpx 42rpx;
	border-radius: 39rpx;
	font-size: 27rpx;
	color: #fefefe;
	text-align: center;
}
.content .part03{
	max-height: 834rpx;
}
.content .part03 .skin{
	padding: 10rpx 30rpx;
	width:600rpx;
	margin:0 auto 34rpx;
	background-color: #dce8ff;
}
.content .part03 .skin image{
	width: 600rpx;
	height: 338rpx;
}
