/**uText**/
.uText{
	margin:0 0 0 30rpx;
	/* border-bottom:1px solid #e6e6e6; */
	line-height:90rpx;
	height:90rpx;
	position:relative;
	clear:both;
}
.uText::before{
	content:'';
	width:100%;
	background: #e6e6e6;
	height:1px;
	position: absolute;
	transform:scale(1,0.5);
	-webkit-transform:scale(1,0.5);
	bottom: 0;
	left:0;
}
.uText.last::before{
	content:none;
}
.uText .key{
	display:inline-block;
	width:170rpx;
	float:left;
}
.uText input{
	height:90rpx;
	line-height:90rpx;
}
.uText .yzm{
	/* border-left:1rpx solid #e6e6e6; */
	width:250rpx;
	text-align:center;
	font-size:28rpx;
	color:#057bff;
	position:absolute;
	right:0;
	top:0;
	height:90rpx;
	line-height:90rpx;
	z-index:9;
}
.uText .address{
	display:inline-block;
	width:520rpx;
	height:140rpx;
	z-index:9;
}

/**detailInfo**/
.detailInfo{
	padding:0 0 0 30rpx;
	background:#fff;
	font-size:28rpx;
}
.detailInfo .uInfo{
	height:90rpx;
	line-height:90rpx;
	/* border-bottom:1px solid #e6e6e6; */
	padding:0 30rpx 0 0;
	position: relative;
}
.detailInfo .uInfo::before{
	content:'';
	width:100%;
	background: #e6e6e6;
	height:1px;
	position: absolute;
	transform:scale(1,0.5);
	-webkit-transform:scale(1,0.5);
	bottom: 0;
	left:0;
}
.detailInfo .uInfo.last::before{
	content:none;
}

.detailInfo .address{
	display:table;
	padding:30rpx 0;
	line-height:45rpx;
	width:100%;
	height:auto;
}
.detailInfo .address-fl{
	display:table-cell;
	width:30%;
}
.detailInfo .address-fr{
	display:table-cell;
	width:70%;
	word-break:break-all;
	text-align:right;
	padding-right:30rpx;
}

/**step**/
.step{
	position:relative;
	padding:0 0 35rpx 40rpx;
	line-height:31rpx;
}
.step::before{
	content:'';
	display:inline-block;
	position:absolute;
	width:1px;
	height:100%;
	left:15rpx;
	top:0;
	background:#e5e5e5;
}
.step::after{
	content:'';
	display:inline-block;
	position:absolute;
	width:20rpx;
	height:20rpx;
	left:6rpx;
	top:0;
	background:#cccccc;
	border-radius:100%;
}
.step .t1{
	color:#999;
	font-size:28rpx;
}
.step .t2{
	color:#999;
	font-size:26rpx;
	margin-top:10rpx;
}

.step-on{
	position:relative;
	padding:0 0 35rpx 40rpx;
	line-height:32rpx;
}
.step-on::after{
	width:26rpx;
	height:26rpx;
	left:-2px;
	background:#0097ff;
	/**box-shadow: 0 0 10px #057bff;**/
	border:6rpx solid #b8d9ff;
}
.step-on .t1{
	color:#0078ff;
}
.step-on .t2{
	color:#0078ff;
}

/** mask **/
.mask{
  width:100%;
  height:100%;
  background:#999;
  filter:alpha(opacity=30);
  -moz-opacity:0.3;
  -khtml-opacity: 0.3;
  opacity: 0.3;
  top:0;
  left:0;
  z-index:9;
  position:fixed;
}

