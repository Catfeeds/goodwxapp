<template name="dialog">
	<view class="dialog {{modalHidden ? 'hide' : ''}}">
	  <view class="mask"></view>
	  <view class="inner">
	  	<view class="{{!sucIcon ? 'hide' : ''}}"> 
		    <icon class="suc marB15" type="success" size="40"/>
		    <view class="tt marB25">提交成功</view>
		</view>    
	    <view class="detail">{{msg}}</view>
	    <button class="btnDialog" bindtap="{{dialogBindTap}}">知道了</button>
	  </view>  
	</view>
</template>