<!--pages/core/club/club.wxml-->
<view class="container">
  <!-- <block wx:for="{{list}}" wx:key="order"> -->
  <block wx:for="{{club}}" wx:key="club">
    <view class="order">
      <view class="order-title" data-name="{{item.name}}" data-index="{{item.number}}" data-intro="{{item.intro}}" data-depart="{{item.depart}}" data-achieve="{{item.achieve}}" data-activity="{{item.activity}}" bindtap="bindEnter">
        <image class="order-state-icon" src="http://image.jmideas.cn/17-9-12/23500653.jpg"></image>
        <view class="order-time">
          <text>{{item.name}}</text>
        </view>
        <!-- <text class="order-state-{{item.status}}">{{item.state}}</text> -->
      </view>
    </view>
  </block>
</view>