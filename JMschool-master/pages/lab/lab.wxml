<!--pages/lab/lab.wxml-->
<view class="container">
  <view class="animate-bounce-up">
    <view class="main-core">
      <navigator class="main-core-item" url="/pages/chatroom/chatroom">
        <image class="core-item-icon" src="/images/lab/chat.png"></image>
        <text class="core-item-name">聊聊</text>
      </navigator>
      <view class="main-core-item" bindtap="bindLab">
        <image class="core-item-icon" src="/images/lab/wait.png"></image>
        <text class="core-item-name">实验室</text>
      </view>
    </view>
  </view>
</view>