/**index.wxss**/
page {
	background: url(data:image/png;base64,) no-repeat center center fixed;

  background-size: cover;
}

.container {
	display: flex;
	flex-direction: column;
	align-items: center;
}

.title {
	font-family: "微软雅黑";
	font-size: 120rpx;
	font-weight: bold;
	color: #ff5d47;
	letter-spacing: 10rpx;
	text-shadow: 1px 1px 0px #f06856, 
				 2px 2px 0px #ec8172, 
				 3px 3px 0px #e6998c, 
				 4px 4px 0px #e3aca2, 
				 5px 5px 0px #dec2ba, 
				 6px 6px 0px #dad3cd, 
				 7px 7px 0px #d6e5e1, 
				 8px 8px 0px #d4f3f1;
	margin: 10rpx auto;
}



.explain {
	color: #353535;
	margin: 10rpx auto;
	font-size: 30rpx;
}


.userinfo-avatar {
	margin-top: 20rpx;
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
}

.menu {
	width: 400rpx;
	justify-content: center;
	flex-direction: row;
	margin: 20rpx auto;
	font-size: 35rpx;
}

.btn {
	text-align: center;
	background-color: #12caf8;
	margin-top: 20rpx;
	height: 100rpx;
	border-radius: 25rpx;
	color: #fff;
	font-size: 40rpx;
	line-height: 100rpx;
	box-shadow: 1px 1px 3px #cccccc;
}

.btn:active {
	background-color: #12a4f8;
	box-shadow: 1px 1px 2px #cccccc;
}

.tips {
	color: #353535;
	margin-top: 20rpx;
	font-size: 20rpx;
	text-align: center;
	height: 15rpx;
	line-height: 15rpx;
	font-weight: 300;
}

.user-list {
	list-style:none;
	background-color: #ecfdfd;
	width: 80%;
	margin: 20rpx auto;
	border: 10rpx solid #12caf8;
	border-radius: 10rpx;
}


.user-item {
	display: flex;
	margin: 30rpx;
}

.user-avatar {
	width: 64rpx;
	height: 64rpx;
	border-radius: 50%;
	margin-right: 20rpx;
}

.info-container {
	width: 100%;
	display: flex;
	flex-direction: column;
}

.niki-name {
	font-size: 30rpx;
	height: 40rpx;
	line-height: 40rpx;

}

.info-box {
	display: flex;
	justify-content: space-between;
}

.info {
	font-size: 20rpx;
}

.money {
	font-size: 20rpx;
	color: #fff;
	background-color: #ff5d47;
	padding: 3rpx;
	border-radius: 4rpx;
}




















