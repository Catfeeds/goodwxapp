<!--logs.wxml-->
<view class="jilu">
	<view class="tag">
		<view class="item fachu {{ faChuOrHuiDa == 0 ? 'active' : ''}}" data-index="0" bindtap="switchFaChuHuiDa">我发出的</view>
		<view class="item huida {{ faChuOrHuiDa == 1 ? 'active' : ''}}" data-index="1" bindtap="switchFaChuHuiDa">我回答的</view>
	</view>
	<view  class="body">
		<image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
		<view>
			<text>我共{{}}的</text>
			<view>
				<view>悬赏额度<text>0</text></view>
				<view>任务数量<text>0</text></view>
			</view>

		</view>
	</view>
	<view class="foot">
		<text>我要投诉</text>
		<text>常见问题</text>
	</view>
</view>
	