/*圆形进度条样式*/
.circle-box {
  position: relative;
  text-align: center;
  height: 20px;
  width: 70px;
}

.circle {
  position: absolute;
  top: 5px;
  left: 0;
  right: 0;
}
.circle-num {
	position: absolute;
	top: 18px;
	font-size: 35px;
	left: 0;
	right: 0;
}