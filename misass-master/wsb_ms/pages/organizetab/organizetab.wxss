/* pages/organizetab/organizetab.wxss */
@import '../../font/iconfont.wxss';
.organizetab>text{
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 38rpx;
}
.organizetab_content{
  height: 200rpx;
  width: 95%;
  margin: 0 auto;
  border: 3rpx solid #bbbbbb;
  display: flex;
}
.organizetab_content>view:nth-of-type(1){
  display: flex;
  flex-direction: column;
  justify-content: center;
  flex: 4;
  margin-left: 50rpx;
}
.organizetab_content>view:nth-of-type(1)>text:first-child{
  margin-bottom:10rpx;
}
.organizetab_content>view:nth-of-type(2){
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
  font-size: 34rpx;
}