.panel {
  display: block;
  margin-top: 10px;
}
.container>view{
    width: 100%;
    height: 60%;
    position: absolute;
}

.other{
  position: fixed;
  display: flex;
  background-color: orange;
  bottom: 0;
  width: 100%;
  justify-content: center;
  height: 70rpx;
  align-items: center;
}
ec-canvas {
  width: 100%;
  height: 60%;
}
.index_detail{
  position: fixed;
  bottom: 25%;
  font-size: 32rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  width: 100%;
}
.index_detail>view{
  background-color: #c6cbd0;
  display: flex;
  justify-content: center;
  flex-direction: column;
  padding: 0rpx 20rpx 25rpx 20rpx;
  flex: 1;
  margin-left: 20rpx;
  cursor:pointer;
}
.index_detail>view>text{
    margin-top:20rpx;
} 
.index_detail>view:last-child{
  margin-right: 20rpx;
}

.table{
  position: fixed;
  bottom: 100rpx;
  background-color: purple;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}