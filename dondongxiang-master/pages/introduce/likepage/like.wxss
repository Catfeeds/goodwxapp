/* pages/introduce/likepage/like.wxss */
.row-page{
    padding: 10px 0;
}
.col-list{
    padding: 10px;
    background: #fff;
    margin-bottom: 10px;
    position: relative;
}
.col-list .label-img{
    float: left;
    width: 50px;
    height: 50px;
}
.col-list .label-img image{
    width: 100%;
    height: 100%;
    display: block;
}
.col-right{
    padding-left: 70px;
    min-height: 50px;
    font-size: 16px;
}
.col-right .name{
    margin-bottom: 8px;
    height: 20px;
    line-height: 20px;
    overflow: hidden;
    padding-top: 3px;
}
.col-right .rectext{
    color: #999;
    font-size: 12px;
    height: 16px;
    line-height: 16px;
    overflow: hidden;
}
.right-phone{
    position: absolute;
    top: 50%;
    right: 10px;
    margin-top: -13px;
}
.right-phone image{
    display: block;
    width: 26px;
    height: 26px;
}