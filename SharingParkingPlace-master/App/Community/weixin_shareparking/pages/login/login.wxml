<import src="../../style/footer.wxml" />

<view class="page__bd">
    <view class="page__hd i-header">
    <view class="i-header-h1 h50">共享车位——</view>
    <view class="i-header-h1">基于图像识别的智能车位共享</view>
    </view>
    <form bindsubmit="formSubmit" bindreset="formReset">

        <view class="weui-cells__title" style='text-align:center'>进入共享车位平台</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">用户名</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入您的账户名" name="accoutName" />
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">密码</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入您的密码" password name="accoutPswd" />
                </view>
            </view>

        </view>
        <view class="weui-cells__tips"></view>
        <view class="weui-btn-area">
            <button class="weui-btn" type="primary" formType="submit">登录</button>
            <button class='weui-btn' type='primary' form-type='reset'>重置</button>
            <view class="reg">
                <navigator url="../register/register" hover-class="other-navigator-hover">如果没有账户，点击注册</navigator>
            </view>
        </view>
    </form>
</view>


<template is="foot" />