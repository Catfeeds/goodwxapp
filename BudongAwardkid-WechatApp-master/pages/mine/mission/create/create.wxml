<!--pages/mine/mission/create/create.wxml-->

<view class="container">
  <form>
    <view class="cells">
      <view class="group">
        <span class="item">
          <input bindinput="onTitleInput" placeholder="请输入关卡名称" confirm-type="done" bindconfirm="onCreateTap" />
        </span>
      </view>
      <view class="controls">
        <button type="primary" bindtap="onCreateTap" form-type="submit" > 创建新关卡 </button>
      </view>
    </view>
  </form>
</view>
