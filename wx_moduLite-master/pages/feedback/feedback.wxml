<!--pages/feedback/feedback.wxml-->
<view class='app'>
<view class='bg'></view>
<form bindsubmit="formSubmit">
  <view class='text-box'>
    <textarea class='textarea' name="feedback" value='{{value}}' auto-focus bindinput='areaInput' placeholder="有什么问题想对我们说的？" />
  </view>
  <view class='opt-box'>
    <button class='submit' formType="submit">提交</button>
  </view>
</form>

<view class='copyright'>
  <text>企牛牛</text> 提供技术支持
</view>
</view>
 