.clean-history{
    background: #fff;
    padding: 20rpx;
    text-align: right;
}

.history-date{
    font-size: 28rpx;
    line-height: 45rpx;
    padding-left: 20rpx;
}
.history-content{
    background: #fff;
    padding: 20rpx;
    border-bottom: 1rpx solid #e7e8eb;
}
.history-content image{
    vertical-align: top;
    width: 210rpx;  
    height: 120rpx;
    border-radius: 10rpx;
}
.history-content text{
    vertical-align: top;
    padding-left: 30rpx;
    font-size: 30rpx;
    color:#494949;
    display: inline-block;
    width: 470rpx;
}
.nohistory{
    text-align: center;
    padding: 30rpx;
}