
.teacher-tab-content{
    height: 100%;
    background: #fff;
}

.teacher-content{
    background: #fff;
    padding: 0 17rpx;
    width: auto;
    overflow-x: hidden;
    display: block;
    
}
.tianchong{
    height: 100rpx;
}

.music-tab{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 80rpx;
    background: #fff;
    font-size: 32rpx;
    background: #fff;
    z-index: 999;
}

.music-tab .m-t{
    width: 50%;
    display: inline-block;
    text-align: center
}

.music-tab text{
    text-align: center;     
    display: inline-block;
    width: 185rpx;
    line-height: 77rpx;
    box-sizing: border-box;
}
.t-active{
     border-bottom: 5rpx solid #358ed7;
     color:  #358ed7;
}

