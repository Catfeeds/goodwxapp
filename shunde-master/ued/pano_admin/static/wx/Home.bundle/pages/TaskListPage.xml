<?xml version="1.0"?>
<Page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="../View.xsd"
      layout="frame"
      background="#FFFFFF">

   <View layout="vertical">
     <!--内容页-->
     <View id="contentContainer" clipToBounds="true" height="fill">
       <!--我的任务-->
       <View id="myTask" layout="vertical"  marginTop="46dp">
         <View width="fill" height="wrap" padding="10 0 10 16" background="#F9F9F9">
           <TextView text="选择要开始处理的需求"  width="fill" height="wrap" fontSize="14dp" fontColor="#888888" />
         </View>
         <View width="fill" height="1dp" background="#E5E5E5"/>
         <View height="fill" clipToBounds="true">
           <ListView id="taskListView" width="fill" height="fill" background="#FFFFFF"/>
         </View>
       </View>
       <!--任务大厅-->
       <include id="myAccount" src="myAccount.xml" gone="true" marginTop="46dp"/>
       <!--我的账户-->
       <include id="taskHall" src="taskHall/page.xml" gone="true" marginTop="46dp"/>
     </View>

     <include id="bottomTab" src="BottomTab.xml"  gravity="bottom"/>
   </View>

    <View width="fill" height="wrap" layout="vertical">
        <View width="fill" height="44dp" background="#FFFFFF">
            <ImageView src="../images/Logo_shot.svg" width="wrap" height="wrap" gravity="center" margin="12 0 12 0"/>
        </View>
        <View width="fill" height="1dp" background="#E5E5E5"/>
    </View>

</Page>
