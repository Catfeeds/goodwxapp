<?xml version="1.0"?>
<View xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="../View.xsd"
      layout="vertical"
      width="fill"
      height="81dp"
      contentGravity="centerVertical"
      id="facilityCellRootView"
      background="#ffffff">

    <View width="wrap" height="wrap" layout="horizontal"  margin="15 0 10 18" >
        <TextView id="facilityName" width="wrap" height="wrap" fontSize="17dp" fontColor="#333333" marginRight="4dp"/>
        <TextView id="facilityNum" width="wrap" height="wrap" fontSize="17dp" fontColor="#333333" />
    </View>

    <ScrollView id="facilityBox" width="fill" height="wrap" layout="horizontal" margin="0 0 10 18"/>

    <View width="fill" height="1dp" background="#E5E5E5" margin="0 0 0 18"/>

</View>
