<?xml version="1.0"?>
<Page id="rootView"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="../View.xsd"
      layout="frame"
      background="#FFFFFF">

    <View layout="vertical">

        <View width="fill" height="wrap" layout="frame" padding="12 0 12 16" background="#FFFFFF">
            <ImageView id="btnBack" width="24dp" height="24dp" src="../images/ic_return.svg"/>
            <TextView id="taskName" text="瑞吉酒店" width="wrap" height="wrap" fontSize="17dp" fontColor="#333333" gravity="center"/>
        </View>
        <View width="fill" height="1dp" background="#E5E5E5"/>

        <View width="fill" height="wrap" background="#FFFFFF" marginTop="16dp" layout="vertical">

            <ImageView width="90dp" height="90dp" src="../images/success.svg" marginTop="75dp" gravity="centerHorizontal"/>

            <TextView width="wrap" height="wrap" text="确认成功" fontColor="#333333" fontSize="20dp"
                      margin="30 0 15 0" gravity="center"/>

            <TextView width="wrap" height="wrap" text="你的需求已确认拍摄完成" fontColor="#888888" fontSize="14dp"
                      margin="0 0 60 0" gravity="center"/>

        </View>

    </View>


</Page>