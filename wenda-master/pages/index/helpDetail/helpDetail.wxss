.circle-wrap {
    position: relative;
    height: 512rpx;
    width: 512rpx;
    margin: 20rpx 119rpx 0;
    border-radius: 50%;
    border: 6rpx solid #fc1;
    box-sizing: border-box;
}
.rotate {
    position: absolute;
    left: -6rpx;
    top: -6rpx;
    height: 512rpx;
    width: 512rpx;
    border-radius: 50%;
    border: 6rpx solid transparent;
    box-sizing: border-box;
}
.circle-wrap image {
    position: absolute;
    left: 252rpx;
    top: 0;
    z-index: -1;
    height: 256rpx;
    width: 256rpx;
    display: block;
}
.rotate-border {
    position: absolute;
    left: 250rpx;
    top: 253rpx;
    height: 6rpx;
    width: 256rpx;
    background-image: linear-gradient(-90deg, #fe7c3b 0%, #fff 100%);
    border-radius: 8rpx;
    z-index: 9;
}
.rotate-border::after {
    content: '';
    display: block;
    position: absolute;
    top: -3rpx;
    left: 250rpx;
    height: 12rpx;
    width: 12rpx;
    background: #ff6128;
    box-shadow: 0 0 10rpx 4rpx #ff6f46;
    border-radius: 50%;
}
.circle-wrap::after {
    content: '';
    display: block;
    position: absolute;
    top: 46rpx;
    left: 46rpx;
    width: 420rpx;
    height: 420rpx;
    border-radius: 50%;
    border: 4rpx solid #fc1;
    box-sizing: border-box;
}
.circle-wrap::before {
    content: '';
    display: block;
    position: absolute;
    top: 83rpx;
    left: 83rpx;
    width: 346rpx;
    height: 346rpx;
    border-radius: 50%;
