page {
	background: #f2f2f2;
}
.wrap {
	overflow: hidden;
}
.header {
	position: relative;
	height: 300rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1501813531427.jpg) no-repeat;
	background-size: contain;
	overflow: hidden;
}
.header-img {
	position: absolute;
	left: 102rpx;
	top: 60rpx;
	width: 140rpx;
	height: 140rpx;
	background: rgba(255, 255, 255, .6);
	box-shadow: 0 2rpx 8rpx 0 rgba(0, 0, 0, .15);
	border-radius: 50%;
	text-align: center;
}
.header-img image {
	margin-top: 7rpx;
	width: 128rpx;
	height: 128rpx;
	border-radius: 50%;
}
.header-name {
	position: absolute;
	top: 210rpx;
	left: 0;
	width: 344rpx;
	text-align: center;
	color: #333;
	font-size: 28rpx;
	line-height: 40rpx;
}
.header-num {
	font-family: PingFangSC-Medium;
	position: absolute;
	top: 115rpx;
	left: 367rpx;
	width: 72rpx;
	color: #333;
	text-align: center;
	font-size: 36rpx;
	line-height: 36rpx;
}
.header-title1 {
	position: absolute;
	top: 161rpx;
	left: 367rpx;
	text-align: center;
	color: #333;
	font-size: 24rpx;
	line-height: 24rpx;
}
.header-num::after {
	content: '';
	display: block;
	position: absolute;
	left: 121rpx;
	top: 16rpx;
	width: 2rpx;
	height: 38rpx;
	background: #fc1;
}
.header-rate {
	font-family: PingFangSC-Medium;
	position: absolute;
	top: 115rpx;
	left: 553rpx;
	width: 80rpx;
	color: #333;
	text-align: center;
	font-size: 36rpx;
	line-height: 36rpx;
}
.header-title2 {
	position: absolute;
	top: 161rpx;
	left: 569rpx;
	text-align: center;
	color: #333;
	font-size: 24rpx;
	line-height: 24rpx;
}
.title {
	line-height: 60rpx;
	color: #333;
	font-size: 28rpx;
	padding-left: 30rpx;
}
.list {
	overflow: hidden;
	margin-bottom: 140rpx;
}
.item {
	position: relative;
	height: 140rpx;
	margin-bottom: 2rpx;
	background: #fff;
	color: #666;
}
.item-img {
	position: absolute;
	top: 20rpx;
	left: 30rpx;
	height: 100rpx;
	width: 100rpx;
	border-radius: 50%;
}
.item-name {
	position: absolute;
	top: 20rpx;
	left: 140rpx;
	font-size: 30rpx;
	line-height: 30rpx;
	color: #333;
}
.item-region {
	position: absolute;
	top: 60rpx;
	left: 140rpx;
	font-size: 24rpx;
	line-height: 30rpx;
}
.item-pai {
	position: absolute;
	top: 60rpx;
	left: 270rpx;
	font-size: 24rpx;
	line-height: 30rpx;
}
.item-pai::before,
.item-pai::after {
	content: '';
	display: block;
	position: absolute;
	left: -16rpx;
	top: 5rpx;
	width: 2rpx;
	height: 20rpx;
	background: #fc1;
}
.item-pai::after {
	left: 88rpx;
}
.item-level {
	position: absolute;
	top: 60rpx;
	left: 376rpx;
	font-size: 24rpx;
	line-height: 30rpx;
}
.item-time {
	position: absolute;
	top: 90rpx;
	left: 140rpx;
	font-size: 24rpx;
	line-height: 30rpx;
	color: #999;
}
.item-price {
	position: absolute;
	top: 20rpx;
	left: 587rpx;
	color: #ff4840;
	font-size: 28rpx;
	line-height: 38rpx;
}
.item-price text {
	font-family: PingFangSC-Medium;
	position: relative;
	top: 2rpx;
	font-size: 36rpx;
	line-height: 40rpx;
}
.item-num {
	position: absolute;
	top: 24rpx;
	left: 681rpx;
	color: #ff4840;
	font-size: 28rpx;
	line-height: 38rpx;
}
.item-color {
	color: #999;
}
.item-num::before {
	content: '';
	display: block;
	position: absolute;
	left: -10.8rpx;
	top: 7.2rpx;
	width: 2rpx;
	height: 20rpx;
	background: #fc1;
}
.item-status {
	position: absolute;
	top: 90rpx;
	left: 632rpx;
	font-size: 24rpx;
	line-height: 32rpx;
	color: #999;
}
.item-status-color {
	color: #57a1f9;
}
.item-status::after {
	content: '';
	display: block;
	position: absolute;
	top: 2rpx;
	left: 82rpx;
	width: 12rpx;
	height: 26rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1501813705316.jpg) no-repeat;
	background-size: contain;
}
.list-empty {
	margin: 280rpx auto 0;
	width: 244rpx;
	height: 280rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1501813667849.jpg) no-repeat;
	background-size: contain;
	overflow: hidden;
}
.list-empty view {
	margin-top: 208rpx;
	text-align: center;
	color: #999;
	line-height: 33rpx;
	font-size: 24rpx;
}
.my-list,
.my-card,
.question,
.my-wallet {
  position: relative;
	width: 750rpx;
	height: 100rpx;
	padding-left: 30rpx;
	box-sizing: border-box;
	color: #333;
	font-size: 30rpx;
	line-height: 100rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1502955390799.jpg) #fff no-repeat 713rpx 37rpx;
	background-size: 16rpx 26rpx;
}
.my-card text,
.my-wallet text {
	position: absolute;
	right: 58rpx;
	top: 30rpx;
	color: #ff4840;
	font-size: 28rpx;
	line-height: 40rpx;
}
.my-wallet text {
  color: #999;
}
.my-list::after,
.my-wallet::after {
	content: '';
	display: block;
	position: absolute;
	left: 30rpx;
	top: 98rpx;
	background: #e5e5e5;
	width: 720rpx;
	height: 2rpx;
}
.question {
	margin-top: 20rpx;
}