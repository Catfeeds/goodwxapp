<view class="wrap" wx:if="{{show}}">
	<view class="header">
		<view class="header-img">
			<image src="{{headerImg ? headerImg : '../../../images/head.png'}}"></image>
		</view>
		<view class="header-name">{{headerName}}</view>
		<view class="header-num">{{headerNum}}</view>
		<view class="header-title1">总局数</view>
		<view class="header-rate">{{headerRate}}%</view>
		<view class="header-title2">胜率</view>
	</view>
  <view class='my-wallet' bindtap="toWalletEv">
    我的钱包
    <text>{{walletMoney}}元</text>
  </view>
  <view class="my-list" bindtap="toListEv">我的订单</view>
  <view class="my-card" bindtap="toCardEv">
		我的优惠券
		<text>{{cardNum}}张可用</text>
  </view>
  <view class="question" bindtap="toQuestionEv">常见问题</view>
