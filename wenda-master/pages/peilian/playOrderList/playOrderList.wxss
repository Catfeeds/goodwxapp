page {
  background: #f2f2f2;
}
.list {
	overflow: hidden;
	margin-bottom: 140rpx;
}
.item {
	position: relative;
	height: 140rpx;
	margin-bottom: 2rpx;
	background: #fff;
	color: #666;
}
.item-img {
	position: absolute;
	top: 20rpx;
	left: 30rpx;
	height: 100rpx;
	width: 100rpx;
	border-radius: 50%;
}
.item-name {
	position: absolute;
	top: 20rpx;
	left: 140rpx;
	font-size: 30rpx;
	line-height: 30rpx;
	color: #333;
}
.item-region {
	position: absolute;
	top: 60rpx;
	left: 140rpx;
	font-size: 24rpx;
	line-height: 30rpx;
}
.item-pai {
	position: absolute;
	top: 60rpx;
	left: 270rpx;
	font-size: 24rpx;
	line-height: 30rpx;
}
.item-pai::before,
.item-pai::after {
	content: '';
	display: block;
	position: absolute;
	left: -16rpx;
	top: 5rpx;
	width: 2rpx;
	height: 20rpx;
	background: #fc1;
}
.item-pai::after {
	left: 112rpx;
}
.item-level {
	position: absolute;
	top: 60rpx;
	left: 400rpx;
	font-size: 24rpx;
	line-height: 30rpx;
}
.item-time {
	position: absolute;
	top: 90rpx;
	left: 140rpx;
	font-size: 24rpx;
	line-height: 30rpx;
	color: #999;
}
.item-price {
	position: absolute;
	top: 20rpx;
	left: 587rpx;
	color: #ff4840;
	font-size: 28rpx;
	line-height: 38rpx;
}
.item-price text {
	font-family: PingFangSC-Medium;
	position: relative;
	top: 2rpx;
	font-size: 36rpx;
	line-height: 40rpx;
}
.item-num {
	position: absolute;
	top: 24rpx;
	left: 681rpx;
	color: #ff4840;
	font-size: 28rpx;
	line-height: 38rpx;
}
.item-color {
	color: #999;
}
.item-num::before {
	content: '';
	display: block;
	position: absolute;
	left: -10.8rpx;
	top: 7.2rpx;
	width: 2rpx;
	height: 20rpx;
	background: #fc1;
}
.item-status {
	position: absolute;
	top: 90rpx;
	left: 632rpx;
	font-size: 24rpx;
	line-height: 32rpx;
	color: #999;
}
.item-status-color {
	color: #57a1f9;
}
.item-status::after {
	content: '';
	display: block;
	position: absolute;
	top: 2rpx;
	left: 82rpx;
	width: 12rpx;
	height: 26rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1501813705316.jpg) no-repeat;
	background-size: contain;
}
.list-empty {
	margin: 280rpx auto 0;
	width: 244rpx;
	height: 280rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1501813667849.jpg) no-repeat;
	background-size: contain;
	overflow: hidden;
}
.list-empty view {
	margin-top: 208rpx;
	text-align: center;
	color: #999;
	line-height: 33rpx;
	font-size: 24rpx;
}