page {
  background: #f2f2f2;
}
.detail-header {
	height: 180rpx;
	background: #fff;
	position: relative;
}
.detail-header-headImg-cot {
	width: 140rpx;
	height: 140rpx;
	background: rgba(255,255,255,0.60);
	box-shadow: 0 2px 8px 0 rgba(0,0,0,0.15);
	border-radius: 70rpx;
	position: absolute;
	top: 20rpx;
	left: 62rpx;
}
.detail-header-headImg {
	width: 128rpx;
	height: 128rpx;
	border-radius: 64rpx;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}
.detail-header-headName {
	font-size: 28rpx;
	color: #333333;
	line-height: 40rpx;
	position: absolute;
	top: 51rpx;
	left: 222rpx;
}
.detail-header-rank {
	font-size: 24rpx;
	color: #666666;
	letter-spacing: 0;
	line-height: 30rpx;
	left: 222rpx;
	top: 106rpx;
	position: absolute;
}
.detail-header-second {
	font-size: 24rpx;
	color: #999999;
	letter-spacing: 0;
	line-height: 30rpx;
	top: 106rpx;
	right: 50rpx;
	position: absolute;
}


.detail-info-header {
	height: 60rpx;
	line-height: 60rpx;
	font-size: 28rpx;
	color: #333333;
	padding-left: 30rpx;
}
.detail-info-content {
	height: 382rpx;
	position: relative;
	background: #fff;
}
.detail-info-item-common {
	width: 220rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	font-size: 28rpx;
	color: #333333;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1501817442405.jpg) no-repeat center center;
	background-size: contain;
	position: absolute;
	top: 20rpx;
	
}
.detail-info-item-spe {
	color: #FFCC11;
	height: 80%;
}
.detail-info-item-left {
	left: 30rpx;
}

.detail-info-item-center {
	left: 265rpx;
}
.detail-info-item-right {
	right: 30rpx;
}
.detail-info-com {
	font-size: 28rpx;
	color: #666666;
	line-height: 40rpx;
	position: absolute;
	left: 35rpx;
  right: 35rpx;
}
.detail-info-com-up {
	top: 111rpx;
}
.detail-info-com-up:after {
	content: '';
	display: block;
	width: 100%;
	background: #DFDFDF;
	height: 1rpx;
	position: absolute;
	top: -10rpx;
}
.detail-info-com-center {
	top: 161rpx;
}
.detail-info-com-bottom {
	top: 211rpx;
}
.detail-info-com-bottom:after {
	content: '';
	display: block;
	width: 100%;
	background: #DFDFDF;
	height: 1rpx;
	position: absolute;
	bottom: -10rpx;
}
.detail-info-coupon {
	top: 272rpx;
}
.detail-info-com-left {
	float: left;
}
.detail-info-com-lineThrough {
	text-decoration: line-through;
}
.detail-info-com-right {
	float: right;
}
.detail-info-price {
	top: 322rpx;
}
.detail-info-com-red {
	color: #FF354E;
}
.detail-coupon {
	width: 120rpx;
	height: 120rpx;
	position: fixed;
	bottom: 160rpx;
	right: 20rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1502876024213.jpg) no-repeat center center;
	background-size: contain;
}

.play-order-result {
	margin-top: 10rpx;
	background: #fff;
	position: relative;
}

.play-order-result.appeal:after {
    content: '';
    position: absolute;
    width: 100rpx;
    height: 100rpx;
    background: url(https://image.qunmee.com/wxapp/qunmee_task1499397780046.jpg) no-repeat;
    background-size: contain;
    right: 10rpx;
    top: 10rpx;
}
.play-order-result-gamePerson {
	height: 88rpx;
	line-height: 88rpx;
	font-size: 28rpx;
	color: #333333;
	border-left: 30rpx solid #fff;
	border-right: 30rpx solid #fff;
	position: relative;
}
.play-order-result-gamePerson:after {
	content: '';
	display: block;
	width: 100%;
	background: #DFDFDF;
	height: 1rpx;
	position: absolute;
	z-index: 1;
	bottom: 0;
}
.play-order-result-cot {
	height: 88rpx;
	line-height: 88rpx;
	font-size: 28rpx;
	color: #333333;
	padding:0 30rpx;
}
.play-order-result-list {
	display: inline-block;
	height: 100%;
	text-align: center;
	width: 168rpx;
	position: relative;
}
.play-order-result-list.split:after {
	position: absolute;
	content: '';
	display: inline-block;
	height: 40%;
	background: #FFCC11;
	width: 2rpx;
	right: 0;
	top: 24rpx;
}
.play-order-result-list-name {
	font-size: 30rpx;
	color: #666666;
}
.play-order-result-list-content {
	font-size: 30rpx;
	color: #FF354E;
}


/*确认结果页面样式*/

.play-time {
  font-size: 34rpx;
  color: #333;
  text-align: center;
  margin-top: 40rpx;
  line-height: 48rpx;
}

.play-end-button {
  width: 600rpx;
  height: 100rpx;
  background: #fc1;
  border-radius: 10rpx;
  font-size: 34rpx;
  color: #333;
  letter-spacing: 0;
  text-align: center;
  line-height: 100rpx;
  margin: 0 auto;
  position: fixed;
  bottom: 90rpx;
  left: 50%;
  margin-left: -300rpx;
}

.play-contract {
  font-size: 28rpx;
  color: #666;
  position: fixed;
  bottom: 30rpx;
  width: 120rpx;
  text-align: center;
  left: 50%;
  margin-left: -60rpx;
}

.play-evidence-text {
  font-size: 28rpx;
  color: #333;
  text-align: center;
  margin: 20rpx;
}

.play-evidence-container {
  width: 420rpx;
  height: 420rpx;
  margin: 0 auto;
}

.play-evidence {
  width: 100%;
  height: 100%;
}

.play-appeal {
  text-align: center;
  margin: 80rpx;
  font-size: 28rpx;
  color: #666;
}

.play-appeal-p {
  margin-top: 20rpx;
}

.kf-modal {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: #000;
  z-index: 9999;
}

.kf-container {
  width: 640rpx;
  height: 640rpx;
  background: #fff;
  -webkit-transform: translate(-50%, -50%);
  top: 50%;
  left: 50%;
  position: absolute;
  border-radius: 20rpx;
}

.kf-p {
  height: 120rpx;
  text-align: center;
  line-height: 120rpx;
  font-size: 32rpx;
  color: #666;
}

.kf-div {
  width: 360rpx;
  height: 360rpx;
  background: red;
  margin: 0 auto;
  margin-top: 40rpx;
}

.kf-img {
  width: 100%;
  height: 100%;
}

.share-modal {
	position: fixed;left: 0;top: 0;right: 0;bottom: 0;background: rgba(0,0,0,.5); z-index:9999
}
.share-modal-container {
	width: 710rpx;
	height: 1166rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1502877473871.jpg) no-repeat;
	background-size: contain; 
	-webkit-transform: translate(-50%,-50%);top: 50%;left: 50%;
	position: absolute;
	border-radius: 20rpx;
}
.share-modal-close {
	width: 80rpx;
	height: 80rpx;
	background: url(https://image.qunmee.com/wxapp/qunmee_task1502877517109.jpg) no-repeat;
	background-size: contain;
	position: absolute;
	bottom: 4rpx;
	left: 50%;
	-webkit-transform: translate(-50%,0%);
}