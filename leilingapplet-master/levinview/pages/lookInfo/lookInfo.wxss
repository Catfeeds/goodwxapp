/* lookInfo.wxss */
@import "../header/header.wxss";
.infoRegister_wrap{
  background: #fff;
  width:100%;
  padding-top:10rpx;
}
.infoRegister{
  background: #fff url(../../img/lookInfoTitle.png) no-repeat left top;
  width:98%;
  height: 900rpx;
  background-size: 100% auto;
  margin:0rpx auto;
  padding-top:180rpx;
}
.infoRegister .info{
   background: #fff url(../../img/info.png) no-repeat left top;
   width:500rpx;
   margin:0 auto;
   height: 240rpx;
   background-size: 600rpx auto;
   font-size: 28rpx;
   color:#4a4271;
   line-height:52rpx;
   padding:160rpx 50rpx 0;
}
.infoRegister .button{
  width:550rpx;
  height: 70rpx;
  margin: 0 auto;
}
.infoRegister .button image{
  width: 184rpx;
  height: 70rpx;
  float: left;
}
.infoRegister .button .reyuyue image{
  float: right;
}
.submitInfo image{
  margin:0 auto;
  width:690rpx;
  height: 410rpx;
  display: block;
}
