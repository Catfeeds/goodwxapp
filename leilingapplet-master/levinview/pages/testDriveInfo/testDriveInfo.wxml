<include src="../header/header.wxml"/>
<view class="container">
  <view class="infoRegister_wrap">
      <view class="infoRegister">
          <view class="people">
              <image src="../../img/4s.png"></image>
          </view>
          <label>
              <text>姓名 : </text>
              <input type="text" placeholder="您的姓名" />
          </label>
           <label>
              <text>电话 : </text>
              <input type="number" placeholder="您的电话号码" maxlength="11" />
          </label>
          <view class="people">
              <image src="../../img/test.png"></image>
          </view>
           <label>
              <text>姓名 : </text>
              <input type="text" placeholder="您的姓名" />
          </label>
          <!--照片  -->
          <view class="uploadPic">
            <image src="../../img/pic.png"></image>
          </view>
          <!--上传照片  -->
          <view class="upload button">
              <image src="../../img/upload.png"></image>
          </view>
          <!--提交  -->
          <view class="submit button">
              <image src="../../img/sb.png" bindtap="submit"></image>
          </view>
          <!--提示信息  -->
          <view class="tishi">
              提示后无法调整,请仔细填写.
          </view>
      </view>
