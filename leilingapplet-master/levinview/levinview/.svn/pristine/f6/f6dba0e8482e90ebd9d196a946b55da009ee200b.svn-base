<!--公共头部-->
<include src="../header/header.wxml"/>
<view class="container">
  <view class="infoRegister_wrap">
      <view class="infoRegister" style="background: #fff url({{imagesUrl}}levin/img/lookInfoTitle.png) no-repeat left top;  background-size: 100% auto;">
          <view class="info" style=" background: #fff url({{imagesUrl}}levin/img/info.png) no-repeat left top;  background-size: 600rpx auto;">
          {{info}}
          </view>
          <view class="button">
               <view class="update">
                 <image src="{{imagesUrl}}levin/img/update.png" bindtap="update"></image>
               </view>
               <view class="reyuyue">
                <image src="{{imagesUrl}}levin/img/reyuyue.png" bindtap="reyuyue"></image>
               </view>
          </view>
          <view class="submitInfo">
              <image src="{{imagesUrl}}levin/img/submitInfo.png" bindtap="submitInfo"></image>
          </view>
      </view>
  </view>
</view>
