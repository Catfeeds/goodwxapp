<!--bookSuccess.wxml-->
<block wx:for="{{infoList}}">
  <view class="list" id="{{item.id}}" bindtap="redirect" data-sta="{{item.status}}">
        <image src="{{item.url}}"></image>
        <text>{{item.text}}</text>
        <block wx:if="{{item.status==1}}">
          <image src="{{imagesUrl}}levin/img/look.png" class="look"></image>
        </block>
  </view> 
</block>
 
