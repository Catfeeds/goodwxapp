<!--map.wxml-->
<view class="M_map_wrap">
  <view class="M_map2">
    <map id="map" longitude="{{longitude}}" latitude="{{latitude}}" markers="{{markers}}" bindmarkertap="markertap" include-points="{{points}}" show-location> </map>
    <image src="{{imagesUrl}}choose4S.png" class="choose4s"></image>
    <scroll-view scroll-y id="scroll">
      <radio-group class="radio-group" bindchange="radioChange">
        <label class="radio" wx:for="{{dealerinfo}}">
                    <view  class="scroll-view-item ">
                      <text class="title" style=" background: url({{imagesUrl}}circle.png) no-repeat left; background-size: 10rpx auto;">{{item.address}}</text>
                      <!-- <text class="con">{{item.tel}}</text> -->
                      <radio value="{{index}}" checked="{{item.checked}}" color="#60379d"/>
                    </view>
                </label>
      </radio-group>
    </scroll-view>
    <image src="{{imagesUrl}}sure.png" class="sure" bindtap="close"></image>
  </view>
</view>