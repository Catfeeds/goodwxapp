<!--testDriveInfo.wxml-->
<include src="../header/header.wxml" />
<view class="container">
  <view class="infoRegister_wrap">
    <view class="infoRegister" style=" background: #fff url({{imagesUrl}}bg.png) no-repeat left top;  background-size: 100% auto;">
      <view class="people">
        <image src="{{imagesUrl}}4s.png"></image>
      </view>
      <label>
              <text>姓名 : </text>
              <input type="text"   bindblur="testSName"/>
          </label>
      <label>
              <text>电话 : </text>
              <input type="number"  maxlength="11"  bindinput="testMobile2" />
          </label>
      <view class="testTel">
        <text>试驾人员电话 : {{mobile}}</text>
      </view>
      <!--照片  -->
      <view class="uploadPic">
        <image src="{{uploadPicURL}}" bindtap="uploadPic"></image>
      </view>
      <!--上传照片  -->
      <view class="upload button">
        <image src="{{imagesUrl}}upload.png" bindtap="uploadPic"></image>
      </view>
      <!--提交  -->
      <view class="submit button">
        <image src="{{imagesUrl}}sb.png" bindtap="submit"></image>
      </view>
      <!--提示信息  -->
      <view class="tishi" style=" background: url({{imagesUrl}}star.png) no-repeat left; background-size: 30rpx auto;">
        提交后无法调整,请仔细填写.
      </view>
    </view>
  </view>
</view>