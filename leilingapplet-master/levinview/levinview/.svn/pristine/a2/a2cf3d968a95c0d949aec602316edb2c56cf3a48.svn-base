/* lookTestDriveInfo.wxss */
@import "../header/header.wxss";

.infoRegister_wrap {
  background: #fff;
  width: 100%;
  padding-top: 5rpx;
}

.infoRegister {
  width: 98%;
  height: 1080rpx;
  margin: 0rpx auto;
}

.infoRegister .testinfo {
  display: block;
  margin: 0 auto;
  width: 600rpx;
  height: 160rpx;
  padding-top: 15rpx;
}

.infoRegister .ti {
  width: 30%;
  float: left;
  color: #000;
  text-align: right;
  margin-right: 2%;
}

.infoRegister  .ti2 {
  width: 62%;
  float: left;
}

.infoRegister .result {
  display: block;
  width: 300rpx;
  height: 100rpx;
  margin-left: 20rpx;
}

.testDriveInfo {
  width: 640rpx;
  height: 800rpx;
  margin: 0 auto;
  font-size: 28rpx;
  color: #4a4271;
  line-height: 55rpx;
  margin-top: -40rpx;
  padding-top: 50rpx;
}

.imgStr {
  width: 560rpx;
  height: 300rpx;
  display: block;
  margin: 0rpx auto;
}

.resultInfo {
  width: 560rpx;
  margin: 0 auto;
  display: block;
  font-size: 26rpx;
  line-height: 40rpx;
}

/*返回首页*/

.goIndex image {
  margin: 5rpx auto;
  width: 180rpx;
  height: 70rpx;
  display: block;
}
