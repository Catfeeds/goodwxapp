<!--testDriveInfo.wxml-->
<include src="../header/header.wxml"/>
<view class="container">
  <view class="infoRegister_wrap">
      <view class="infoRegister" style=" background: #fff url({{imagesUrl}}levin/img/bg.png) no-repeat left top;  background-size: 100% auto;">
          <view class="people">
              <image src="{{imagesUrl}}levin/img/4s.png"></image>
          </view>
          <label>
              <text>姓名 : </text>
              <input type="text" placeholder="您的姓名" />
          </label>
           <label>
              <text>电话 : </text>
              <input type="number" placeholder="您的电话号码" maxlength="11" />
          </label>
          <view class="people">
              <image src="{{imagesUrl}}levin/img/test.png"></image>
          </view>
           <label>
              <text>姓名 : </text>
              <input type="text" placeholder="您的姓名" />
          </label>
          <!--照片  -->
          <view class="uploadPic">
            <image src="{{imagesUrl}}levin/img/pic.png"></image>
          </view>
          <!--上传照片  -->
          <view class="upload button">
              <image src="{{imagesUrl}}levin/img/upload.png"></image>
          </view>
          <!--提交  -->
          <view class="submit button">
              <image src="{{imagesUrl}}levin/img/sb.png" bindtap="submit"></image>
          </view>
          <!--提示信息  -->
          <view class="tishi" style=" background: url({{imagesUrl}}levin/img/star.png) no-repeat left; background-size: 30rpx auto;">
              提示后无法调整,请仔细填写.
          </view>
      </view>
  </view>
</view>
