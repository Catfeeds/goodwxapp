/* bookSuccess.wxss */
.list{
  position: relative;
  width: 100%;
  height: 360rpx;
  margin-top:40rpx;
  font-family:"微软雅黑";
  
}
.list image{
  width: 750rpx;
  height: 360rpx;
  display: block;
}
 .list text{
  width: 750rpx;
  height: 360rpx;
  position: absolute;
  left:0;
  top:0;
  color:#fff;
  line-height: 360rpx;
  text-align: center;
  font-size: 54rpx;
  font-weight: 900;
  letter-spacing: 10rpx;
} 
.list .look{
  width: 215rpx;
  height: 70rpx;
  position: absolute;
  left:267rpx;
  top:250rpx;
}