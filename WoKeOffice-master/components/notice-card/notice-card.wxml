<view class="notice-wrapper">
  <view class="card-header">
    <image class="manager-avatar"></image>
    <view class="header-mid-info">
      <view class="manager-info">{{managerUsername}} 发布了通知</view>
      <view class="teamname">{{teamName}}</view>
    </view>
    <view class="header-tail">
      <view class="update-date">{{updateDate}}</view>
      <view class="visibility">{{visibility}}</view>
    </view>
  </view>
  <view class="notice-content">{{noticeContent}}</view>
</view>
