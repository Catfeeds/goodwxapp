<view class="item-wrapper">
  <view class="file-type">{{fileType}}</view>
  <view class="file-wrapper">
    <image class="file-icon"></image>
    <view class="file-info-wrapper">
      <view class="file-name">{{fileName}}</view>
      <view class="upload-info-wrapper">
        <view class="upload-time">{{uploadTime}}</view>
        <view class="upload-user">{{uploadUser}}</view>
        <view class="upload-word">上传</view>
      </view>
    </view>
    <view class="download-icon"></view>
  </view>
</view>
