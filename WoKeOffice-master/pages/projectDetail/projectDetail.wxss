.tab-head-item{
  margin: 0 60rpx;
}

.tab-head-item.active:after{
  width: 65rpx;
}

.timeline-terminal-wrapper{
  width: 175rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
}

.timeline-terminal-word{
  width: 90rpx;
  height: 26rpx;
  line-height: 26rpx;
  font-size: 26rpx;
  margin-left: 20rpx;
  text-align: start;
  color: #666666;
}

.timeline-wrapper{
  margin-left: 20rpx;
  width: 45rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.timeline-terminal-1{
  width: 6rpx;
  height: 100rpx;
  background: -webkit-linear-gradient(top, #EFEFF5, #66CC00);
}

.timeline-terminal-2{
  width: 6rpx;
  height: 100rpx;
  background: -webkit-linear-gradient(top, #66CC00, #EFEFF5);
}