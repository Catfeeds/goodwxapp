/* rankinglist.wxss */
page{
    width:100%;
    height:100%;
    background: #221638;
    padding: 0;
    margin: 0;
    font-family: "Hiragino Sans GB";
}
/*小助手*/
.contact_btn {
    width: 258rpx;
    height: 84rpx;
    background: transparent;
    border: none;
    outline: none;
    position: fixed;
    left: -58rpx;
    top:32rpx;
    z-index: 99;
}
button[class="contact_btn"]::after {
    border: 0;
}
.contact_btn image{
    width:258rpx;
    height:84rpx;
    border: none;
    position: absolute;
    top: 0;
    left: 0;
}
/*个人信息*/
.info-box{
    width:750rpx;
    height:750rpx;
    margin-top:-418rpx;
    background-image: radial-gradient(#54397E 50%, rgba(84,57,126,0.00) 99%);
    position: relative;
}
.info-box .camera{
    width:100rpx;
    height:100rpx;
    position: absolute;
    bottom:210rpx;
    right:22rpx;
}
.info-box .book_name{
    width:100vw;
    text-align: center;
    height:30rpx;
    position: absolute;
    bottom:270rpx;
    left: 0;
    font-size: 28rpx;
    color: #FFFFFF;
    letter-spacing: 1.17rpx;
    font-weight:700;
}
.info-box .user-box{
    width:500rpx;
    height:108rpx;
    position: absolute;
    bottom:125rpx;
    left:125rpx;
}
.info-box .user-box .head_img_wrap{
    width:102rpx;
    height:108rpx;
    position: absolute;
    left:80rpx;
    top: 0;
}
.info-box .user-box .head_img{
     width:90rpx;
     height:90rpx;
     border-radius: 45rpx;
     position: absolute;
     left:86rpx;
     top:6rpx;
}
.info-box .user-box .nickname{
    width:180rpx;
    height:50rpx;
    position: absolute;
    left:205rpx;
    top:6rpx;
    font-size: 30rpx;
    color: #CBB0F6;
    letter-spacing: 1.25rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.info-box .user-box .user_score{
    width:180rpx;
    height:30rpx;
    position: absolute;
    left:205rpx;
    top:50rpx;
    font-size: 30rpx;
    color: #CBB0F6;
    letter-spacing: 1.25rpx;
}
.info-box .user-box .user_score text{
    font-size: 50rpx;
    color: #FFFFFF;
}
.info-box .bar-box{
    width:90rpx;
    height:90rpx;
    position: absolute;
    left:520rpx;
    bottom:120rpx;
    font-size: 30rpx;
    color: #CBB0F6;
    letter-spacing: 1.25rpx;
}
.info-box .bar-box .bar_play{
    width:90rpx;
    height:90rpx;
}
.info-box .bar-box .bar_time{
    font-size: 28rpx;
    color: #CBB0F6;
    letter-spacing: 1.17rpx;
    height:28rpx;
    width:100rpx;
    position: absolute;
    left:100rpx;
    top:8rpx;
}
.info-box .bar_text{
    width:750rpx;
    height:25rpx;
    text-align: center;
    font-size: 24rpx;
    color: #CBB0F6;
    letter-spacing: 1rpx;
    position: absolute;
    bottom:35rpx;
    letter-spacing: 1.25rpx;
}
.ranking .rank_title{
    width:750rpx;
    height:80rpx;
}
.ranking .ranking-box{
    width:750rpx;
    height:630rpx;
    overflow: scroll;
}
.ranking .list{
    display: flex;
    height:90rpx;
    line-height:90rpx;
}
.ranking .text{
    font-size: 32rpx;
    text-indent:2em;
    color: #CBB0F6;
    margin-top:260rpx;
}
.ranking .list .index{
    font-family: "Hiragino Sans GB";
    width:150rpx;
    text-align: center;
    font-size: 24rpx;
    color: #80888F;
    letter-spacing: 1rpx;
}
.ranking .list .index_1{
    width:150rpx;
    font-family: "Hiragino Sans GB";
    text-align: center;
    font-weight:500;
    font-size: 36px;
    color: #CBB0F6;
    letter-spacing: 1.5rpx;
}
.ranking .list .nickname{
    font-family: "Hiragino Sans GB";
    width:225rpx;
    text-align: center;
    font-size: 24rpx;
    color: #80888F;
    letter-spacing: 1rpx;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.ranking .list .score{
    font-family: "Hiragino Sans GB";
    width:290rpx;
    text-align: center;
    font-size: 24rpx;
    color: #CBB0F6;
    letter-spacing: 1rpx;
    font-weight:500;
}
.ranking .list .tag{
    font-family: "Hiragino Sans GB";
    width:290rpx;
    text-align: center;
    font-size: 24rpx;
    color: #80888F;
    letter-spacing: 1rpx;
}
.ranking .list .tag image{
    width:70rpx;
    height:70rpx;
    margin:auto;
    margin-top:10rpx;
}
