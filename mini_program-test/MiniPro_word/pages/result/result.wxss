/* rankinglist.wxss */
page{
    width:100%;
    height:100%;
    background: #003249;
    padding: 0;
    margin: 0;
}
.pages .board{
    width:100vw;
    height:880rpx;
    position: absolute;
    padding-top:200rpx;
}
.pages .board .board_img{
    width:100vw;
    height:880rpx;
}
.pages .board .flag{
    font-family: "Hiragino Sans GB";
    font-size: 46rpx;
    color: #FFFFFF;
    letter-spacing: 4rpx;
    line-height: 50rpx;
    position: absolute;
    width:100vw;
    text-align: center;
    left: 0;
    top: 450rpx;
    font-weight:700;
}
.pages .board .flag_p{
    font-family: "Hiragino Sans GB";
    font-size: 46rpx;
    color: #FFFFFF;
    letter-spacing: 4rpx;
    line-height: 50rpx;
    position: absolute;
    width:100vw;
    text-align: center;
    left: 0;
    top: 650rpx;
    font-weight:700;
}
.pages .board .sen{
    font-family: "Hiragino Sans GB";
    font-size: 26rpx;
    color: #FFFFFF;
    letter-spacing: 2rpx;
    line-height: 40rpx;
    position: absolute;
    width:360rpx;
    text-align: center;
    left: 195rpx;
    top: 520rpx;
}
.pages .board .sen_p{
    font-family: "Hiragino Sans GB";
    font-size: 26rpx;
    color: #FFFFFF;
    letter-spacing: 2rpx;
    line-height: 40rpx;
    position: absolute;
    width:360rpx;
    text-align: center;
    left: 195rpx;
    top: 720rpx;
}
.pages .board .score{
    font-family: "Hiragino Sans GB";
    font-size: 76rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    position: absolute;
    width:100vw;
    text-align: center;
    top: 635rpx;
    font-weight:700;
}
.pages .board .score_p{
    font-family: "Hiragino Sans GB";
    font-size: 76rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    position: absolute;
    width:100vw;
    text-align: center;
    top: 835rpx;
    font-weight:700;
}
.pages .board .high_score{
    position: absolute;
    width:124rpx;
    height:51rpx;
    top:580rpx;
    right:63rpx;
}
.pages .board .high_score_p{
    position: absolute;
    width:124rpx;
    height:51rpx;
    top:780rpx;
    right:63rpx;
}
.user-box{
    position: absolute;
    width:100vw;
    height:180rpx;
    text-align: center;
    padding-top:50rpx;
}
.user-box .head-box{
    width:158rpx;
    height:158rpx;
    margin:auto;
    margin-bottom:30rpx;
    position: relative;
}
.user-box .head_img_wrap{
    width:158rpx;
    height:158rpx;
}
.user-box .head_img{
    position: absolute;
    top:12rpx;
    left:15rpx;
    width:130rpx;
    height:130rpx;
    border-radius: 65rpx;
}
.user-box text{
    font-family:"Hiragino Sans GB";
    font-size: 30rpx;
    color: #78CAED;
    letter-spacing: 1.25rpx;
}
.button-box{
    display: flex;
    /*padding-top:830rpx;*/
    position: absolute;
    top:832rpx;
    left: 0;
}
.button-box .rank_img{
    width:180rpx;
    height:100rpx;
    margin-left:65rpx;
    margin-right:40rpx;
 }
.button-box .save_img{
    width:180rpx;
    height:100rpx;
    margin-right:40rpx;
}
.button-box .replay_img{
    width:180rpx;
    height:100rpx;
}
/*转发按钮*/
.share_btn{
    font-family:"Hiragino Sans GB";
    width:100vw;
    position: absolute;
    top:966rpx;
    left: 0;
    text-align: center;
}
.share_btn button{
    border:0;
    background: transparent;
    font-size: 24rpx;
    color: #78CAED;
    letter-spacing: 2rpx;
    font-weight:200;
    /*text-decoration:underline;*/
}
.share_btn button text{
    /*display: block;*/
    padding-bottom:3rpx;
    border-bottom:1rpx solid #78CAED;
}
.share_btn button::after{
    border:0;
}
.app-box{
    width:100vw;
    height:128rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    background: #2B2B2B;
    display: flex;
}
.app-box .logo{
    width:100rpx;
    height:68rpx;
    margin:30rpx 20rpx 30rpx 20rpx;
}
.app-box .load{
    width:180rpx;
    height:60rpx;
    margin:34rpx 20rpx 34rpx 60rpx;
}
.app-box text{
    font-family: "Hiragino Sans GB";
    display: block;
    height:128rpx;
    line-height:128rpx;
    font-size: 24rpx;
    color: #80888F;
    letter-spacing: 0;
}
/*查看详情*/
.info_wrap{
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,.5);
    width:100vw;
    height:100vh;
}
.info{
    width:500rpx;
    height:800rpx;
    position: relative;
    margin: auto;
    margin-top:180rpx;
}
.info_bg{
    width:500rpx;
    height:800rpx;
}
.button_off{
    width:140rpx;
    height:79rpx;
    position: absolute;
    top:-30rpx;
    right:-70rpx;
}
.info view.total{
    position: absolute;
    font-size:32rpx;
    color: #2B2B2B;
    top:170rpx;
    left: 30rpx;
    width:440rpx;
    height:560rpx;
    overflow: scroll;
    /*text-indent: 2em;*/
}
.info view.total::-webkit-scrollbar {
    display: none;
}
.qr_code{
    width:300rpx;
    height:300rpx;
    margin:auto;
    margin-left:70rpx;
}
.contact_btn {
    width: 160rpx;
    height: 80rpx;
    background: transparent;
    border: none;
    outline: none;
    position: fixed;
    right: 0;
    top:35rpx;
}
button[class="contact_btn"]::after {
    border: 0;
}
.contact_btn image{
    width:160rpx;
    height:80rpx;
    border: none;
    position: absolute;
    top: 0;
    left: 0;
}
