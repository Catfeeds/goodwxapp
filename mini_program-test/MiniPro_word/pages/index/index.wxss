/* rankinglist.wxss */
page{
  width:100%;
  height:100%;
  background: #003249;
  padding: 0;
  margin: 0;
}
.pages{
  /*background-image: linear-gradient(-180deg, #272D39 0%, #384152 100%);*/
  /*background-image: url('http://file.abctime.com/shouye_bg.png');*/
  background-size: cover;
  text-align: center;
  height:100vh;
  padding: 0;
  margin: 0;
}
.heart-box{
  width:160rpx;
  height:80rpx;
  text-align: left;
  position: fixed;
  right:0;
  top: 32rpx;
  color: #ffffff;
}
.heart-box_p{
  width:160rpx;
  height:80rpx;
  text-align: left;
  position: fixed;
  right:0;
  top: 132rpx;
  color: #ffffff;
}
.heart-box image,.heart-box_p image{
  width:160rpx;
  height:80rpx;
}
.heart-box text,.heart-box_p text{
  position: absolute;
  display: block;
  width:80rpx;
  line-height:80rpx;
  text-align: center;
  top: 0;
  left:75rpx;
  color:#78CAED;
  font-size:32rpx;
  font-family:"Hiragino Sans GB";
  letter-spacing:2rpx;
  font-weight:600;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.refresh{
  width:110rpx;
  height:20rpx;
  text-align: left;
  position: fixed;
  right:13rpx;
  top: 120rpx;
  color: #ffffff;
}
.refresh_p{
  width:110rpx;
  height:20rpx;
  text-align: left;
  position: fixed;
  right:13rpx;
  top: 220rpx;
  color: #ffffff;
}
.heartModal{
  position: fixed;
  top:30vh;
  left:0;
  width:100vw;
  height:320rpx;
  line-height:350rpx;
  border-radius: 40rpx;
  text-align: center;
}
.heartModal image{
  width:542rpx;
  height:320rpx;
  margin: auto;
}
.heartModal view{
  position: absolute;
  left: 240rpx;
  top: 160rpx;
  width:270rpx;
  font-family: "Hiragino Sans GB";
  font-size: 32rpx;
  color: #2B2B2B;
  letter-spacing: 2rpx;
  line-height: 40rpx;
}
.logo{
  width:100vw;
  position: absolute;
  top: 0;
  left: 0;
}
.logo_p{
  width:100vw;
  position: absolute;
  top: 170rpx;
  left: 0;
}
.foot-box{
  display: flex;
  position: fixed;
  bottom: 50rpx;
  left: 0;
}
.test-box{
  margin-left:50rpx;
  width: 440rpx;
  height:100rpx;
}
.rank-box{
  margin-left:30rpx;
  width:180rpx;
  height:100rpx;
}
.contact_btn {
  width: 160rpx;
  height: 80rpx;
  background: transparent;
  border: none;
  outline: none;
  position: fixed;
  right: 0;
  bottom:214rpx;
}
button[class="contact_btn"]::after {
  border: 0;
}
.contact_btn image{
  width:160rpx;
  height:80rpx;
  border: none;
  position: absolute;
  top: 0;
  left: 0;
}
.tomain{
  width:160rpx;
  height:80rpx;
  position: fixed;
  left:0;
  top: 32rpx;
  color: #ffffff;
}
.tomain_p{
  width:160rpx;
  height:80rpx;
  position: fixed;
  left:0;
  top: 132rpx;
}