/* rankinglist.wxss */
page{
  width:100%;
  height:100vh;
  background: #1EB7DE;
  background-image: url('http://file.abctime.com/result_bg.png');
  background-size: 100% 100%;
  color:#ffffff;
  text-align: center;
  font-weight:500;
}
.header{
  display: block;
  padding-top:35rpx;
  font-size: 46rpx;
  color: #FFFFFF;
  letter-spacing: 1.64rpx;
  /*line-height: 50rpx;*/
}
.content{
  margin-top:215rpx;
  font-size: 36rpx;
  color: #FFFFFF;
  letter-spacing: 1.57rpx;
  /*line-height: 50rpx;*/
}
.ipx{
  margin-top:300rpx;
}
.content view.box{
  text-align: left;
  width:580rpx;
  margin:auto;
  margin-top:25rpx;
  font-size: 26rpx;
  color: #FFFFFF;
  letter-spacing: 1.04rpx;
  line-height:40rpx;
}
.content .text{
  text-indent: 2em;
}
.footer{
  position: absolute;
  left:25rpx;
  bottom:20rpx;
  width:700rpx;
  margin:auto;
}
.footer image{
  display: inline-block;
  width:100rpx;
  height:100rpx;
  margin-right:30rpx;
}
.footer text{
  display: inline-block;
  vertical-align: top;
  line-height:100rpx;
  font-size: 26rpx;
  color: #2B2B2B;
  letter-spacing: 1rpx;
}
.hidden{
  display: none;
}