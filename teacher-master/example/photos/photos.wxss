.photo_text {
  line-height: 20px;
  color: #5b5b5b;
  font-size: 14px;
  padding: 10px 15px;
}


.photo_tip::before {
  font-family: 'iconfont';
  font-size: 22px;
  color: #1e90ff;
  content: '\e727';
}






.photo-op-fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #F8F8F8	;
  box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
  z-index: 100;
  color: #282828;
  display: flex;
  justify-content: center;
  align-items: center;
}





.main-profile-flex {
  display: flex;
  padding-top: 10px;
  padding-bottom: 10px;
  padding-left: 20px;
  padding-right: 20px;
}

.category-flex {
  display: flex;

}

.category-cell-title{
  font-size: 11px;
  color:#484848;
}
.category-cells {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

