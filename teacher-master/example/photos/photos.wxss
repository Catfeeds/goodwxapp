.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  /*padding: 200rpx 0;*/
  box-sizing: border-box;
  background: #f0f4f3;
}

.container.withtab {
  margin: 105rpx 0 0 0;
  /*top: 105rpx;*/
}

.container .feed-item {
  width: 100%;
  padding: 30rpx 30rpx 20rpx;
  margin: 9rpx 0 0rpx 0;
  background: #fff;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
}

.feed-source {
  height: 75rpx;
  display: flex;
  justify-content: space-between;
  padding: 5px 15px;
}

.feed-source .avatar image {
  /*position: absolute;*/
  display: inline-block;
  width: 75rpx;
  height: 75rpx;
  border-radius: 50rpx;
  top: 10rpx;
  vertical-align: middle;
}

.feed-source text {
  /*position: absolute;*/
  display: inline-block;
  height: 75rpx;
  line-height: 75rpx;
  vertical-align: middle;
  margin: 0 0 0 15rpx;
  color: black;
  font-size: 30rpx;
}

.feed-source .item-more {
  height: 45rpx;
  text-align: center;
}
