
.category-flex {
  display: flex;
}

.category-cell-title{
  font-size: 15px
}
.category-cells {
  flex: 3;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
border-right: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
  font-size:15px
}

.main-profile-more::after {
 font-family:'iconfont';
font-size:20px;
content:'\e603';
