  content: '\e727';
}






.photo-op-fixed {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #F8F8F8	;
  box-shadow: 0 0 2px rgba(0, 0, 0, 0.1);
  z-index: 100;
  color: #282828;
  display: flex;
  justify-content: center;
  align-items: center;
}
