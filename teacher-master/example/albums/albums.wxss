.feed-source {
  height: 75rpx;
  display: flex;
  justify-content: space-between;
  padding: 5px 15px;
}

.feed-source .avatar image {
  /*position: absolute;*/
  display: inline-block;
  width: 75rpx;
  height: 75rpx;
  border-radius: 50rpx;
  top: 10rpx;
  vertical-align: middle;
}

.feed-source text {
  /*position: absolute;*/
  display: inline-block;
  height: 75rpx;
  line-height: 75rpx;
  vertical-align: middle;
  margin: 0 0 0 15rpx;
  color: black;
  font-size: 30rpx;
}

.feed-source .item-more {
  height: 45rpx;
  text-align: center;
}
