page{
  background-color: #ececec;
  color:#4A494A;
  font-size:36rpx;
}
.login_box{
    width:100%;
    height:100%;
}
.login{
    display:flex;
    padding-left:60rpx;
    background:#fff;
}
.login view{
    line-height:220rpx;
}
.userinfo-avatar {
  width:128rpx;
  height:128rpx;
  border-radius:50%;
  vertical-align:midden;
  display:inline-block;
  overflow:hidden;
  margin-top:50rpx;
  margin-right:20rpx;
}
.login_img{
    width:100%;
    height:100%;
    text-align:center;
    padding-top:60rpx;
}
.empty{
   width:578rpx;
   height:522rpx;
}
.login_info{
    width:100%;
    height:428rpx;
    background:#fff;
    text-align:center;
}
.login_info text{
    display:block;
    margin:16rpx 0;
}
.login_content{
    background:#fff;
    height:176rpx;
    box-shadow:0 2px 4px 0 #e6e6e6;
}
.icon_img{
    width:32rpx;
    height:38rpx;
    display:inline-block;
    margin:0 32rpx 0 42rpx;
    vertical-align:middle;
}
.icon_set{
    width:42rpx;
    height:40rpx;
    display:inline-block;
    margin:0 32rpx 0 42rpx;
    vertical-align:middle;
}
.icon_right{
    width:32rpx;
    height:38rpx;
    display:inline-block;
    float:right;
    margin-right:60rpx;
    position:absolute;
    right:0;
    top:24rpx;
}
.set_item{
    height:84rpx;
    line-height:84rpx;
    position: relative;
}
.my_order{
    border-bottom:1px solid #f0f0f0;
}