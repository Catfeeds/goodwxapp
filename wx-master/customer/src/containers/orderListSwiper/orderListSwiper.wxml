<view class="noList" hidden="{{orderList.length>0 || !loaded}}">
	<image src="../../../static/images/img_noList@3x.png" class="icon"></image>
	<p>暂无接车单！</p>
</view>

<view class="swiperContainer" hidden="{{orderList.length==0 || !loaded}}">
     <div class="left" wx:if="{{hasLeft}}"></div>
		 <view style="position:relative">
     <swiper circular="{{circular}}" bindchange="bindchange">
      <block>
        <swiper-item wx:for-items="{{orderList}}" wx:key="{{item.id}}" >
          <view class="slide-item">
               <view class="garageName">{{item.garageName}}</view>
               <div class="title">
                 接车单
               </div>
							 <image src="../../../static/images/label_process.png" class="tag" wx:if="{{item.status==1 || item.status==2 || item.status==3}}"></image>
               <image src="../../../static/images/label_wait.png" class="tag tag2" wx:if="{{item.status==4}}"></image>
               <image src="../../../static/images/label_ok.png" class="tag tag2" wx:if="{{item.status==5}}"></image>
               <image src="../../../static/images/label_refuse.png" class="tag tag2" wx:if="{{item.status==6 }}"></image>
               <div class="no">NO:{{item.orderNumber}}(车主联)</div>
               <div class="time">接车时间：{{item.createdDateStr}}</div>
               <div class="carInfo">
                    <div>车型/车款：{{item.autoModelChnName}}</div>
                    <div>车牌号：{{item.vehicleLicenceCode}}</div>
               </div>
                <div class="checkRes" wx:if="{{!item.abnormalCount&&(item.status=='4'||item.status=='5'||item.status=='6')}}">
                    <span>检查结果：</span>正常
               </div>
               <div class="checkRes" wx:elif="{{item.abnormalCount&&(item.status=='4'||item.status=='5'||item.status=='6')}}">
                   <span>检查结果：</span>{{item.abnormalCount}}个异常项目
              </div>
              <div class="checkRes" wx:else><span>检查结果：</span>未出</div>
               <div class="toDetailBtn" bindtap="switchToDetail" data-itemId="{{item.id}}">查看详情</div>
          </view>
        </swiper-item>
      </block>
    </swiper>
		   <div class="indicator">{{current+1}}/{{totalSize}}</div>
		</view>
    <div class="right" wx:if="{{hasRight}}"></div>
    <view class="switchBtn" bindtap="switchToList">查看更多>></view>
</view>
