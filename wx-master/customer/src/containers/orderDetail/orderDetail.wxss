.topNav {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding: 30rpx 0;
    background: #fff;
		margin:20rpx 30rpx 90rpx;
}

.topNav_item {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    font-size: 28rpx;

    text-align: center;
}

.topNav_item text {
    display: inline-block;
    position: relative;
    z-index: 1;
}

.topNav_item text:before {
    content: '';
    display: block;
    margin: 0 auto 16rpx;
    width: 18rpx;
    height: 18rpx;
    background: #e2e4e6;
    border-radius: 100%;
}

.topNav_item text:after {
    content: '';
    display: block;
    position: absolute;
    top: 8rpx;
    right: 35rpx;
    width: 140rpx;
    height: 1rpx;
    background: #e2e4e6;
    z-index: -1;
}

.topNav_item text.on:before {
    background: #f60;
}

.topNav_item text.on:after {
    background: #f60;
}

.topNav_item:first-child text {
    z-index: 5;
}

.topNav_item:nth-of-type(2) text {
    z-index: 4;
}

.topNav_item:nth-of-type(3) text {
    z-index: 3;
}

.topNav_item:nth-of-type(4) text {
    z-index: 2;
}

.topNav_item:first-child {}

.topNav_item:last-child {}

.topNav_item:first-child text:after {
    display: none;
}
.topNav_item text.on{
   color:#f60
}
