<template name="slider_self">
  <view class="slider_self">
        <view class="slider_bar">
               <view class="slider_bar_scale slider_bar_scale_long"><text>0%</text></view>
               <view class="slider_bar_scale"></view>
               <view class="slider_bar_scale"></view>
               <view class="slider_bar_scale"></view>
               <view class="slider_bar_scale"></view>
               <view class="slider_bar_scale slider_bar_scale_long"><text>50%</text></view>
               <view class="slider_bar_scale"></view>
               <view class="slider_bar_scale"></view>
               <view class="slider_bar_scale"></view>
               <view class="slider_bar_scale"><text>100%</text></view>
        </view>
        <view class="slider_thumb" style="left:{{offsetLeft}}px" bindtouchstart='_sliderTouchStart' bindtouchmove='_sliderTouchMove' bindtouchend='_sliderTouchEnd' ></view>
  </view>
</template>
