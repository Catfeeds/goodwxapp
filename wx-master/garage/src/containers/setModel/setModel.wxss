.radio_group_warp{
    background:#fff;
}
.radio_group_warp .name{
    margin-bottom:12rpx;
    font-family:.PingFangSC-Regular;
    font-size:32rpx;
    color:#333333;
    text-align:left;
}
.radio_group_warp .text{
    font-family:.PingFangSC-Regular;
    font-size:24rpx;
    color:#999999;
    letter-spacing:0px;
    text-align:left;
}
.radio_item{
    display:-webkit-box;
    display:-webkit-flex;
    display:flex;
    -webkit-box-align:center;
    -webkit-align-items:center;
            align-items:center;
    -webkit-box-pack:justify;
    -webkit-justify-content:space-between;
            justify-content:space-between;
    padding:26rpx 0;
    margin:0 26rpx;
    border-bottom:1px solid #e2e4e6;
}
.radio_item label{
    -webkit-box-flex:1;
    -webkit-flex:1;
            flex:1;
}