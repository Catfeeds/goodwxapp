/* pages/details/details.wxss */

#login_frame .with-line {
	width:95%;font-size: 16px;
	color: #999;
	margin: 0 auto;
	position: relative;
	text-align: center
}

#login_frame .with-line:before {
	content: "";
	border-top: 1px solid #ededed;
	display: block;
	position: absolute;
	width:35%;
	top: 10px;
	left: 0
}

#login_frame .with-line:after {
	content: "";
	border-top: 1px solid #ededed;
	display: block;
	position: absolute;
	width:35%;
	top: 10px;
	right: 0
}
