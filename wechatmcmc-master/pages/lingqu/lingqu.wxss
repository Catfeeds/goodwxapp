/* lingqu.wxss */
.top{
  width:100%;
  height:30px;
  text-align:center;
  font-family:"微软雅黑";
  border-top:1px solid #F8F8F8;
  background-color:#ffffff;
}
.youhui{
  width:95%;
  height:80px;
  margin-top:10px;
  margin-left:2.5%;
  background-color: #ffffff;
  border-radius: 9px;

}
.left{
  width:70%;
  height:80px;
  float:left;
}
.right{
  width:30%;
  height:80px;
  font-size:18px;
  text-align: center;
  line-height: 80px;
  background-color:#0CEDAB;
  color:#ffffff;
  float:right;
  border-radius: 0px 9px 9px 0px;
}
.img{
  width:40%;
  height:80px;
  float:left;
  text-align:center;
  line-height: 80px;
}
.cent{
  width:60%;
  height:80px;
  margin-left:40%; 

}
.c_top{
  width:100%;
  height:40px;
  padding-top:5px;
  font-size:12px;

}
.c_bottom{
  width:100%;
  height:40px;
  
  color:#0CEDAB;
  font-size:10px;
}
 .cro_left_top,.cro_right_top,.cro_left_bottom,.cro_right_bottom{         
            width: 5px;
            height: 5px;
             background: #D9D6D6; 
            
        }

.cro_left_top{
    border-radius:0px 0px 9px 0px;
}

.cro_right_top{   
  float:right;
  margin-top:-80px;
    border-radius:0px 0px 0px 9px;
}

.cro_left_bottom{
    margin-top:70px;
    border-radius:0px 9px 0px 0px;
}
.cro_right_bottom{
  margin-top:-5px;
  float:right;
  margin-right:0px;
    border-radius:9px 0px 0px 0px;
}
