.wei{
 margin-top:10px;
  margin-left:2.5%;
  width:95%;
  height:110px;
  border:1px solid #FF5902; 
  background-color:#ffffff;
  border-radius: 10px;
}
.y-top{
  width:100%;
  height:75px;
}
.y-bottom{
  width:100%;
  height:35px;
  border-radius: 0px 0px 9px 9px;
  background-color: #FF5902;
  color:#ffffff;
  font-size:13px;
  
  line-height: 35px;
 
}
.y-left{
  width:30%;
  height:75px;
  color:#FF5902;
  text-align: center;
  line-height: 75px;
  font-size:18px;
  float:left;
}
.y-right{
  width:70%;
  height:75px;
  color:#FF5902;
  font-size:18px;
  float:right;
  text-align: center;
  line-height: 75px;
}


