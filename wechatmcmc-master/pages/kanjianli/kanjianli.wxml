<view class="page" style="margin-bottom:50px;" xmlns:wx="http://www.w3.org/1999/xhtml">
    <view class="page__bd">
        <view class="weui-cells__title"></view>
        <view class="logo_header">
            <image class="logo" src="{{info.logo}}" />
        </view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">姓名</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input">{{info.name}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">性别</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input">{{info.xingbie}}</text>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">年龄</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input">{{info.bron}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">工作经验</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input">{{info.jingyan}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">期望职位</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input">{{info.zhiwei}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">期望薪资</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input">{{info.xinzi}}</text>
                </view>
            </view>

            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">期望区域</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input">{{info.quyu}}</text>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">联系电话</view>
                </view>
                <view class="weui-cell__bd">
                    <text class="weui-input">{{info.phone}}</text>
                </view>
                
            </view>
            
           
            <view style='background-color:#F8F8F8;width:100%;height:10px;' >
           
                <view  style='background-color:#F8F8F8;font-size:17px; padding-left:13px;position: relative;display: -webkit-box;display: -webkit-flex; display: flex;-webkit-box-align: center;-webkit-align-items: center;align-items: center;'>
                    <view class="weui-label">自我介绍</view>
                </view>
               
            </view>
            <view class='weui-cell_input' style="padding: 17px 15px; position: relative;display: -webkit-box;display: -webkit-flex; display: flex;-webkit-box-align: center;-webkit-align-items: center;align-items: center;" >
                  <text style="font-size:17px;">{{info.jieshao}}</text>
            </view>
        </view>
        
    </view>
	
<view class="foot" >
    
        <view class="left" style="line-height:{{height}}px"   bindtap="shuaxin">
           刷新
        </view>

        <view class="right" style="line-height:{{height}}px" bindtap="xiugai">
         编辑
        </view>

        
	</view>
