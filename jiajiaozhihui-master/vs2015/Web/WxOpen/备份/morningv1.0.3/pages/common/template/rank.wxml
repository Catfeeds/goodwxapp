<template name="rank" >
    <view class="modele " hidden="{{top3.length<=0 }}">
        <view class="weui-panel habit-panel">
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_small-appmsg ">
                    <view class="weui-cells habit-cells">
                        <block wx:for="{{top3}}">
                            <view class="weui-cell weui-cell_access habit-cell" href="javascript:;">
                                <view class="weui-cell__hd babit-rank">
                                    <view class="babit-rank__sn">{{item.row}}</view><image style="width:40px;height:40px;" src="{{item.headImgUrl}}"></image>
                                </view>
                                <view class="weui-cell__bd weui-cell_primary">
                                    <view class="habit-rank__nickName">{{item.nickName}}</view>
                                    <view class="habit-rank__desc">已坚持持{{item.finish}}天</view>
                                </view>
                                <view class="weui-cell__ft">
                                    <image style="width:40px;height:40px;" src="/pages/image/rank{{item.row}}.png"></image>
                                </view>
                            </view>
                        </block>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <view class="modele" style="margin-top:15px;" hidden="{{top.length<=0}}">
        <view class="weui-panel habit-panel">
            <view class="weui-panel__bd">
                <view class="weui-media-box weui-media-box_small-appmsg ">
                    <view class="weui-cells habit-cells">
                        <block wx:for="{{top}}">
                            <view class="weui-cell weui-cell_access habit-cell" href="javascript:;">
                                <view class="weui-cell__hd babit-rank">
                                    <view class="babit-rank__sn">{{item.row}}</view><image style="width:40px;height:40px;" src="{{item.headImgUrl}}"></image>
                                </view>
                                <view class="weui-cell__bd weui-cell_primary">
                                    <view class="habit-rank__nickName">{{item.nickName}}</view>
                                    <view class="habit-rank__desc">已坚持持{{item.finish}}天</view>
                                </view>
                            </view>
                        </block>
                    </view>
                </view>
            </view>
        </view>
    </view>
</template>
