<view class="page">
  <view class="page__bd">
    <view class="weui-cells">
      <block wx:for="{{groupList}}">
        <view bindtap="bindSelect" data-groupId="{{item.id}}" class="weui-cell weui-cell_access ">
            <view class="weui-cell__hd"></view>
            <view class="weui-cell__bd">{{item.group_name}}</view>
            <view class="weui-cell__ft"></view>
        </view>
      </block>
    </view> 
  </view>
</view>