/**app.wxss**/
.container {
   background: #f2f2f2;
   font-family:'微软雅黑'; 
   overflow-x: none;
   box-sizing: border-box;
} 
.fr{
  float: right;
}
.footer-space{
  height: 100rpx;
}
.common-footer{
  position: fixed;
  display: flex;
  left: 0;bottom: 0;
  width: 100%;
  height: 100rpx;
  background: #fff;
  z-index: 11;
}
.nav-item-gtoup{
  flex: 1;
  text-align: center;
   font-size: 20rpx;
}
.ico-img-coin{
  display: block;
  margin:12rpx auto;
  width: 38rpx;
  height: 42rpx;
}
.ico-text-active{
  color: #c8535b;
}
.ico-text{
  color: #666;
}
.ico-img-plus{
  width:110rpx;
  height: 110rpx;
  transform: translateY(-30rpx);
  border-radius: 50%;
}
.eos-btn{
   position: fixed;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  border-radius: 40rpx;
  left: 20rpx;
    font-size: 29rpx;
  width: 710rpx;
  cursor: pointer;
  text-align: center;
}
.navigator-hover{
  background: none;
}