.cancellation-background {
  width: 100%;
  height: 330rpx;
}

.cancellation-text {
  width: 100%;
  text-align: center;
  padding-top:90rpx;
}

.cancellation-money{
  /* font-family: PingFang-SC-Medium; */
  font-size: 88rpx;
  color: #000000;
  letter-spacing: -2.12rpx;
  line-height: 88rpx;
}

.cancellation-name {
  /*font-family: PingFangSC-Regular;*/
  font-size: 32rpx;
  color: #323232;
  letter-spacing: -0.36rpx;
}

.cancellation-notice {
  width: 100%;
}

.cancellation-notice-list {
  height: 102rpx;
  line-height: 102rpx;
  /* font-family: PingFangSC-Regular; */
  font-size: 30rpx;
  color: #323232;
  letter-spacing: -0.41rpx;
}

.cancellation-notice-list-f {
  height: 102rpx;
  line-height: 102rpx;
  /* font-family: PingFangSC-Regular; */
  font-size: 26rpx;
  color: #323232;
  letter-spacing: -0.41rpx;
}

.level-image {
  width: 61rpx;
  height: 27rpx;
  vertical-align: middle;
}
.cancellation-cover{
  width:100%; 
  height:100%;
  position:fixed; 
  top :0px;
  opacity:0.8;
  background-color:#000;
  /* opacity:0.8; */
  text-align:center;
}
.cancellation-box{
  opacity:1;
  width:587rpx;
  margin:30% auto;
  border-radius: 12rpx; 

  position: absolute;
  top: 0%;
  left:10%;
}

.cover-banner{
  width:587rpx;
  height:287rpx;
}

.cancellation-box-text{
  /* font-family: PingFangSC-Regular; */
  font-size: 32rpx;
  color: #323232;
  letter-spacing: -0.77rpx;
  padding:20rpx;
}

.cancellation-box-button{
  height: 100rpx;
  bottom: 0;
  /* font-family: PingFangSC-Regular; */
  font-size: 32rpx;
  letter-spacing: -0.77rpx;
  line-height: 100rpx;
  text-align: center;
}

.cancellation-box-button-close{
  background: #EAEAEA;
  color: #787878;
  width:50%;
}

.cancellation-box-button-open{
  background: #36BE7A;
  color: #FFFFFF;
  width:50%;
}

.cancellation-success{
  height:100%;
}
.cancellation-success-img{
  text-align:center;
  margin-top:170rpx;
}
.cancellation-success-image{
  width:235rpx; 
  height:197rpx;
}
.cancellation-success-title{
  height:56rpx;
  line-height:56rpx;
  text-align: center;
}
.cancellation-success-title-money{
  /* font-family: PingFang-SC-Regular; */
  font-size: 40rpx;
  color: #FF6149;
  letter-spacing: -0.96rpx;
  display:inline;
}
.cancellation-success-title-yes{
  /* font-family: PingFang-SC-Regular; */
  font-size: 40rpx;
  color: #000000;
  letter-spacing: -0.77rpx;
  line-height: 56rpx;
  display:inline;
}
.cancellation-success-prompt{
  /* font-family: PingFang-SC-Regular; */
  font-size: 28rpx;
  color: #787878;
  letter-spacing: -0.68rpx;
  text-align: center;
}
.cancellation-success-button{
  width:540rpx;
  height:76rpx;
  margin: 20px auto;
  line-height: 76rpx;
  text-align: center;
  background: #36BE7A;
  border-radius: 69rpx;
  /* font-family: PingFang-SC-Medium; */
  font-size: 34rpx;
  color: #FFFFFF;
  letter-spacing: 1rpx;
}

