page{
  padding: 0 0 40rpx 0;
  min-width: 640rpx;
  color: #333;
  background-color: #fff;
  font-size: 32rpx;
}
.bottom{
  margin-bottom: 30rpx;
}
.time{
  text-align: center;
  height: 100rpx;
  line-height: 100rpx;
  color: #666;
  font-size: 36rpx;
}
.time text{
  color: #FE5850;
}
.content{
  padding: 0 12rpx;
}
.content .header{
  display: flex;
  flex-flow: row wrap;
  justify-content: space-around;
  border-bottom: 6rpx solid #C2D7E9;
  color: #09375A;
  font-size: 36rpx;
}
.section{
  margin:0;
  height: 88rpx;
  line-height: 88rpx;
  border: none;
  border-bottom: 2rpx dotted #D8D8D8;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-around;
}
.section .item{
  width: 25%;
}
.item.width{
  width: 22%;
}
.section .arrow{
  width: 6%;
}
.section .change{
  width: 9%;
}
.section image{
  vertical-align: middle;
}
.arrow image{
  height: 60rpx;
  width: 60rpx;

}
.change image{
  height: 36rpx;
  width: 48rpx;
}
.infoBox{
  width: 100%;
  margin-top: 14rpx;
}
.infoBox .info{
  padding: 6rpx 20rpx;
  font-size: 28rpx;
  color: #8B8682;
  line-height: 50rpx;
}
.infoBox .info text{
  color: #696969;
}


































