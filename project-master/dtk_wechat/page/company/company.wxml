<view class="listBox" wx:if="{{admin_user_role==2}}">
  <!-- 循环开始 -->
  <view class="list-main-lists" wx:for="{{msgList}}"  wx:key="id" id="{{item.id}}"  data-authflag="{{item.auth_flag}}"  data-userrole="{{item.user_role}}"  bindtap="listClick" bindtouchend="touchend"  bindlongtap="mylongtap">
    <view class="company-left">
      <view class="avatar" wx:if="{{item.user_logo_url !== ''}}"><image  src="{{item.user_logo_url}}"></image></view>
      <view class="avatar" wx:else><image  src="{{item.user_logo_url}}"></image></view>
      <view class="" wx:if="{{item.user_role==1}}">系统管理员</view>
      <view class="" wx:elif="{{item.user_role==2}}">公司管理员</view>
      <view class="" wx:elif="{{item.user_role==3}}">注册用户</view>
      <view class="" wx:elif="{{item.user_role==4}}">风控初审</view>
      <view class="" wx:elif="{{item.user_role==5}}">风控复核</view>
    </view>
    <view class="tel">电话：{{item.user_id}}</view>
    <view class="">姓名：{{item.name}}</view>
  </view>
  <loading hidden="{{loadingHidden}}">
  加载中...
  </loading>
  <action-sheet hidden="{{actionSheetHidden}}" bindchange="actionSheetbindchange">
      <block>
          <action-sheet-item>
            <picker ontap="goTorole" class="picker" bindchange="bindChange" value="{{Index}}" range="{{Data}}" range-key="name">
                <view >
                  角色
                </view>
            </picker>
          </action-sheet-item>
      </block>
      <block>
        <action-sheet-item bindtap="goTorefuse">移除</action-sheet-item>
      </block>
      <action-sheet-cancel class="cancel">取消</action-sheet-cancel>
  </action-sheet>
</view>
