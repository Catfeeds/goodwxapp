/* zhengenru 20170731 */
.list-title{
  padding:20rpx 0;
  background: #fff;
  box-shadow: 0 10rpx 10rpx #d2d3d3;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 1;
}
.list-title-box{
  padding:0 30rpx;
}
.list-main{
  padding: 200rpx 30rpx 20rpx 30rpx;
}
.list-main-lists{
  background: #fff;
  margin-bottom: 30rpx;
  overflow: hidden;
  position: relative;
  box-shadow:0 10rpx 10rpx #d2d3d3;
  padding:10rpx 20rpx 20rpx 20rpx;
  border-radius: 10rpx;
}
.ticketList-footer{
  padding-top: 5rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  padding-left:70rpx;
}
.ticketList-mian-list{
  float: left;
  width: 50%;
  line-height: 52rpx;
}
.color-red{
  color: #d43836;
}
.list-main-lists-left-text{
  float: left;
  margin-right: 10rpx;
}
.list-main-lists-left-text image{
  width: 45rpx; 
  height: 45rpx;
  float: left;
  margin-top: 5rpx;
}
.ticketList-title{
  padding-bottom: 10rpx;
  font-weight: bold;
}
.bank{
  float: left;
}
.bank-width{
  width:460rpx;
  text-overflow:ellipsis;
  white-space:nowrap;
  overflow: hidden;
}

/* 2017-11-03 左右业务增加全部/我的 tab */
.swiper-tab{  
  width: 100%;  
  text-align: center;  
  line-height: 100rpx;
  background: #d43836;
  position: fixed;
  top: 0;
  z-index: 1;
}  
.swiper-tab-list{
  font-size: 32rpx;  
  display: inline-block;  
  width: 50%;  
  color: #fff;  
}  
.on{
  color: #fff;  
  border-bottom: 2rpx solid #fff;
  padding: 0 10rpx 10rpx;
  }
.swiper-box{ 
  padding-top: 150rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  display: block; 
  height: 100%;
  overflow: hidden;
  }
.addbusiness{
  position: absolute;
  width: 165rpx;
  height: 70rpx;
  top: 65rpx;
  left: 50%;
  margin-left: -82rpx;
}
.addbusiness image{
  width: 165rpx;
  height: 70rpx;
}
.list-all-title{
  height: 60rpx;
  border-bottom: 2rpx solid #EEEFEF;
  padding: 0 20rpx;
  overflow: hidden;
}
.list-all-title-box{
  float: left;
  font-size: 28rpx;
  color: #3e5696;
}
.list-main-all{
  height: 260rpx; 
  background: #fff;
  margin-bottom: 30rpx;
  overflow: hidden;
  position: relative;
  border-radius: 10rpx;
  box-shadow:0 10rpx 10rpx #d5d5d5;
}
.list-all-title-time{
  float: right;
  font-size: 24rpx;
  color: #666666;
  font-weight: normal;
}
.list-all-main{
  padding: 0 20rpx 0 100rpx;
  position: relative;
}
.list-all-main-image{
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
  position: absolute;
  left: 20rpx;
  top: 10rpx;
}
.list-all-main-title{
 font-size: 28rpx;
 font-weight:bold; 
 padding-top: 10rpx;
 overflow: hidden;
 height:60rpx;
}
.list-all-main-title text{
  float: left;
  padding-right: 10rpx;
}
.list-all-main-box-text{
  float: left;
  width: 33.3%;
  text-align: left;
}
.list-all-main-box-title{
  font-size: 24rpx;
  color: #888888;
  height: 50rpx;
  line-height: 50rpx;
}
.list-all-main-box-info{
  font-size: 44rpx;
  color: #ea5414;
}
.list-all-main-box-info text{
  font-size: 24rpx;
}
.list-all-main-me{
  padding: 20rpx 20rpx 0 100rpx;
}
.list-all-main-title text.list-all-title-time{
  float: right;
  font-weight: normal;
}
.fontsize{
  font-size: 32rpx;
}
.height-me{
  height: 220rpx;
}
.ticketList-title-text{
  color: #3e5696;
}