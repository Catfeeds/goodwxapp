.view__register,
.view__join,
.view__none,
.view__noClaim,
.view__forward,
.view__group {
  min-height: 100vh;
  background: url("https://imgcdn.youhaodongxi.com/weapp/index-bg-a51e4cdb6a.png");
  background-size: 100% auto;
  padding: 200rpx 24rpx 0rpx 24rpx;
}
.view__main {
  width: 702rpx;
  padding: 60rpx 70rpx;
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
  position: relative;
}
.view__mainOne {
  height: 1006rpx;
}
view.cut {
  width: 60rpx;
  height: 8rpx;
  background: #ffe46f;
  margin-top: 30rpx;
  position: absolute;
  left: 45%;
}
image.bananer {
  width: 560rpx;
  height: 260rpx;
  margin-bottom: 60rpx;
  margin-top: 20rpx;
}
image.bananerT {
  width: 500rpx;
  height: 400rpx;
  margin-top: 20rpx;
  padding: 20rpx 30rpx;
}
image.moments {
  height: 160rpx;
  width: 160rpx;
  display: block;
  margin: 0 auto;
  margin-top: 180rpx;
}
.enjoy {
  text-align: center;
  padding-top: 30rpx;
}
.lifetext {
  padding-top: 30rpx;
}
view.apply {
  padding: 0 80rpx;
}
.groManage {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.view__register {
  padding-bottom: 44rpx;
}
.view__register .view__main {
  border-radius: 20rpx;
}
.view__join .view__main >.arrow {
  margin: 0 auto;
  width: 20rpx;
  height: 20rpx;
  border-right: 4rpx solid #991f5c;
  border-bottom: 4rpx solid #991f5c;
  transform: rotate(45deg);
}
.view__join .view__main >image {
  width: 560rpx;
  height: 560rpx;
}
.view__none .view__main >image {
  width: 300rpx;
  height: 240rpx;
  margin: 140rpx auto 90rpx auto;
}
.view__noClaim .view__main >image {
  width: 300rpx;
  height: 240rpx;
  margin: 140rpx auto 90rpx auto;
}
.view__forward .view__main >image {
  width: 300rpx;
  height: 240rpx;
  margin: 140rpx auto 90rpx auto;
}
.view__forward .view__main >.button {
  margin-top: 114rpx;
}
.view__group {
  padding: 20rpx 24rpx 0rpx 24rpx;
}
.view__group .group__headerTop {
  height: 200rpx;
  padding: 50rpx 30rpx;
  background: url("https://imgcdn.youhaodongxi.com/weapp/cover-bg-cb0fbda9f4.png");
  background-size: cover;
  border-radius: 20rpx 20rpx 0 0;
}
.view__group .group__headerBottom {
  height: 120rpx;
  background: #fff;
  border-radius: 0 0 20rpx 20rpx;
}
.view__group .manageBtn {
  width: 140rpx;
  background: rgba(255,255,255,0.7);
}
.view__group .icon-gold {
  width: 72rpx;
  height: 72rpx;
}
.view__group .arrow {
  width: 20rpx;
  height: 20rpx;
  border-top: 4rpx solid #9e7f5a;
  border-right: 4rpx solid #9e7f5a;
  transform: rotate(45deg);
}
.view__group .group__mineGold {
  border-left: 2rpx solid #ffe46f;
}
.view__group .group__body {
  min-height: calc(100vh - 360rpx);
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
}
.view__group .group__bodyNav {
  height: 100rpx;
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
  border-bottom: 2rpx solid #ddd;
}
.view__group .group__bodyMain {
  padding-bottom: 20rpx;
  background: #fff;
  border-radius: 0 0 20rpx 20rpx;
}
.view__group .group__bodyNavTab {
  flex: 1;
  font-size: 26rpx;
  font-weight: bold;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #999;
}
.view__group .group__bodyNavTab--active {
  color: #333;
  position: relative;
}
.view__group .group__bodyNavTab--active::after {
  content: '';
  width: 40rpx;
  height: 4rpx;
  background: #333;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 0;
}
.view__group .list__bodyNavTab--active {
  color: #333;
}
.view__group .group {
  z-index: 1;
  position: relative;
}
.view__group .group::after {
  display: block;
  content: '';
  width: 2rpx;
  height: calc(100% - 60rpx);
  background: #ccc;
  position: absolute;
  top: 80rpx;
  left: 69rpx;
}
.view__group .group >text {
  display: block;
  font-size: 26rpx;
  font-weight: bold;
  padding-top: 30rpx;
  padding-bottom: 10rpx;
  margin-left: 44rpx;
}
.view__group .group--empty::after {
  display: none;
}
.view__group .feed {
  position: relative;
  z-index: 10;
}
.view__group view.quite {
  width: 100%;
  text-align: center;
  color: #b3b3b3;
  padding: 20rpx;
}
.view__group #dailySignModal,
.view__group #signSuccessModal {
  position: relative;
}
.view__group #dailySignModal .modal__header image,
.view__group #signSuccessModal .modal__header image {
  width: 560rpx;
  height: 130rpx;
  position: absolute;
  top: -90rpx;
  left: 50%;
  transform: translateX(-50%);
}
.view__group #dailySignModal .modal__body,
.view__group #signSuccessModal .modal__body {
  padding: 100rpx 84rpx 60rpx 84rpx;
}
.view__group #signSuccessModal .modal__body {
  padding-top: 76rpx;
}
.view__group #signSuccessModal .modal__body > image {
  width: 120rpx;
  height: 120rpx;
}
.view__group #couponModal .modal__body {
  padding: 40rpx 55rpx;
  text-align: center;
}
.view__group #couponModal .modal__body .coupon {
  height: 160rpx;
  background: #fff0b0;
  position: relative;
}
.view__group #couponModal .modal__body .coupon::before,
.view__group #couponModal .modal__body .coupon::after {
  content: '';
  display: block;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background: #fff;
  position: absolute;
  left: 162rpx;
}
.view__group #couponModal .modal__body .coupon::before {
  top: -20rpx;
}
.view__group #couponModal .modal__body .coupon::after {
  bottom: -20rpx;
}
.view__group #couponModal .modal__body .coupon__left {
  height: 100%;
  width: 180rpx;
}
.view__group #couponModal .modal__body .coupon__right {
  height: 100%;
  border-left: 4rpx dashed #fff;
  padding-left: 20rpx;
  text-align: left;
  line-height: 32rpx;
}
.view__other image {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}
.joinCoupon {
  height: 72rpx;
  width: 240rpx;
}
.enter {
  padding: 30rpx 20rpx 0 20rpx;
}
.homeBtn {
  width: 100%;
  height: 100rpx;
  background: url("https://imgcdn.youhaodongxi.com/weapp/homeBtn-7613ca9a67.png");
  background-size: 100% 100%;
  position: relative;
  border-radius: 8rpx;
  padding: 0 30rpx;
}
.homeArrow {
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 30rpx;
  right: 30rpx;
}
.list__body {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
}
.question {
  height: 26rpx;
  width: 26rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.rule {
  margin-bottom: -20rpx;
}
