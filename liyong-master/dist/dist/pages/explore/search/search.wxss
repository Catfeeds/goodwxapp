#page {
  min-height: 100vh;
}
.search {
  width: 100%;
  heihgt: 108rpx;
  font-size: 26rpx;
  padding: 20rpx 30rpx;
}
input {
  height: 68rpx;
  border: 2rpx solid #b3b3b3;
  position: relative;
  padding-left: 60rpx;
}
.searchIcon {
  height: 30rpx;
  width: 30rpx;
  position: absolute;
  top: 40rpx;
  left: 50rpx;
}
.deleteIcon {
  height: 28rpx;
  width: 28rpx;
  position: absolute;
  top: 40rpx;
  right: 50rpx;
  z-index: 5;
}
.searchMain {
  background: #f3f3f3;
  height: 1100rpx;
  padding: 0 30rpx;
}
.goods {
  border-top: 2rpx solid #e8e8e8;
  padding: 20rpx 0;
  display: flex;
  justify-content: space-between;
}
.goodsImg {
  width: 200rpx;
  height: 200rpx;
}
.goodsMes {
  width: 470rpx;
}
.goodsTex {
  font-size: 28rpx;
  display: block;
}
.user {
  height: 50rpx;
  display: flex;
  line-height: 50rpx;
  margin-top: 20rpx;
}
.userImg {
  width: 50rpx;
  height: 50rpx;
  margin-left: -10rpx;
  border: 2rpx solid #fff;
  border-radius: 50%;
}
.userNum {
  font-size: 20rpx;
  margin-left: 10rpx;
  color: #666;
}
.goodsPrice {
  display: flex;
  color: #991f5c;
  margin-top: 20rpx;
}
.vip {
  padding-right: 20rpx;
}
.profit {
  padding-left: 20rpx;
}
.small {
  font-size: 20rpx;
}
.big {
  font-size: 32rpx;
}
.noGoods {
  font-size: 28rpx;
  color: brownishGrey;
  display: inline-block;
  width: 252rpx;
  height: 80rpx;
  text-align: center;
  margin-top: 292rpx;
  position: relative;
  left: 50%;
  margin-left: -106rpx;
}
