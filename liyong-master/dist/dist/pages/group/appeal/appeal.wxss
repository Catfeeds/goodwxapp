#page {
  min-height: 100vh;
  background: url("https://imgcdn.youhaodongxi.com/weapp/index-bg-a51e4cdb6a.png");
  background-size: cover;
  padding: 20rpx 24rpx 0 24rpx;
}
.main {
  height: calc(100vh - 20rpx);
}
.cover {
  height: 200rpx;
  background: url("https://imgcdn.youhaodongxi.com/weapp/cover-bg-cb0fbda9f4.png");
  background-size: 100% auto;
  border-radius: 20rpx 20rpx 0 0;
  padding: 50rpx 30rpx;
}
.content {
  height: calc(100vh - 200rpx);
  background: #fff;
  border-radius: 20rpx 20rpx 0 0;
  margin-top: -20rpx;
  padding: 180rpx 150rpx 0 150rpx;
}
.uploadImage {
  display: block;
  width: 400rpx;
  height: 400rpx;
  margin: 0 auto;
}
.help {
  display: block;
  margin-top: 114rpx;
}
.overText {
  display: block;
  width: 90%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
