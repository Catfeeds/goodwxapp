#page {
  padding: 30rpx;
}
.actionBtn {
  width: 200rpx;
  height: 50rpx;
  border-radius: 25rpx;
  border: 2rpx solid #999;
  font-size: 22rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.actionBtn--active {
  background: #ffe46f;
  border: 0;
}
.feeds__label {
  font-size: 28rpx;
  padding-left: 20rpx;
  border-left: 8rpx solid #ffe46f;
}
