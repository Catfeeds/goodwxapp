.saleSupport__header {
  height: 160rpx;
  padding: 40rpx 30rpx;
  background: #ffe46f;
}
.saleSupport__header .content {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
  padding-left: 20rpx;
  border-left: 2rpx solid #9e7f5a;
  font-size: 26rpx;
  min-height: 8rpx;
}
.saleSupport__header .button {
  padding: 0;
  width: 140rpx;
  height: 60rpx;
  line-height: 56rpx;
  background: transparent;
  border: 2rpx solid #333;
}
.saleSupport__merchType {
  display: flex;
  height: 80rpx;
  padding: 20rpx;
  font-size: 28rpx;
  background: #f3f3f3;
}
.saleSupport__content {
  font-size: 28rpx;
}
.saleSupport__content .uploadImages {
  margin-top: 20rpx;
  margin-left: -4rpx;
  margin-right: -4rpx;
}
.saleSupport__content .uploadImage {
  float: left;
  width: 166rpx;
  height: 166rpx;
  margin: 0 4rpx;
}
