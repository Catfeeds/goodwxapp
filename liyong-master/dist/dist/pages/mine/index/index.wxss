#dailySignModal,
#signSuccessModal {
  position: relative;
}
#dailySignModal .modal__header image,
#signSuccessModal .modal__header image {
  width: 560rpx;
  height: 130rpx;
  position: absolute;
  top: -90rpx;
  left: 50%;
  transform: translateX(-50%);
}
#dailySignModal .modal__body,
#signSuccessModal .modal__body {
  width: 630rpx;
  padding: 100rpx 84rpx 60rpx 84rpx;
}
#signSuccessModal .modal__body {
  padding-top: 76rpx;
}
#signSuccessModal .modal__body > image {
  width: 120rpx;
  height: 120rpx;
}
#groupGiftModal {
  text-align: center;
}
#groupGiftModal .modal__body {
  width: 630rpx;
  padding: 40rpx 55rpx;
}
#groupGiftModal .button {
  margin: 0 auto;
  border: 0;
  padding: 0;
  width: 440rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
}
#groupGiftModal .coupon {
  height: 160rpx;
  background: #fff0b0;
  position: relative;
}
#groupGiftModal .coupon::before,
#groupGiftModal .coupon::after {
  content: '';
  display: block;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background: #fff;
  position: absolute;
  left: 162rpx;
}
#groupGiftModal .coupon::before {
  top: -20rpx;
}
#groupGiftModal .coupon::after {
  bottom: -20rpx;
}
#groupGiftModal .coupon__left {
  height: 100%;
  width: 180rpx;
}
#groupGiftModal .coupon__right {
  height: 100%;
  border-left: 4rpx dashed #fff;
  padding-left: 20rpx;
  text-align: left;
  line-height: 32rpx;
}
