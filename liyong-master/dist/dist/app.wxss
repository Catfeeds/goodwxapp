.shell {
  background: #e8e8e8;
  animation: shimmer 1.2s ease-in-out infinite;
}
@keyframes shimmer {
  0% {
    opacity: 0.8;
  }
  50% {
    opacity: 0.6;
  }
  100% {
    opacity: 0.8;
  }
}
button {
  padding: 0;
  border: 0;
}
button::after {
  content: none;
}
.button {
  font-size: 26rpx;
  line-height: 1.25em;
  background: #fff;
  border: 2rpx solid #b3b3b3;
  border-radius: 8rpx;
  box-sizing: border-box;
  padding: 10rpx 18rpx;
  text-align: center;
}
.button--primary {
  border-color: #fff;
  background: linear-gradient(to left, #ffe46f, #ffec9a);
}
.button--disabled {
  border: 0;
  background: #ccc;
}
.button--disabledGradient {
  border: 0;
  background: linear-gradient(to left, #ccc, #dbdbdb);
}
.button--large {
  font-size: 28rpx;
  line-height: 36rpx;
  font-weight: bold;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.button--small {
  font-size: 24rpx;
  line-height: 32rpx;
  padding: 0 15rpx;
}
.button--noBorder {
  border: 0;
}
.button--noBorder::after {
  content: none;
}
button.button::after {
  content: none;
}
.button--border {
  border-color: #e8e8e8;
  background: #e8e8e8;
}
page {
  border-top: 2rpx solid #f3f3f3;
  font-size: 32rpx;
  line-height: 1.25em;
  color: #333;
}
view,
input,
image,
textarea {
  box-sizing: border-box;
}
.textarea-placeholder,
.input-placeholder {
  color: #999;
}
view {
  word-break: break-all;
}
img {
  max-width: 100%;
}
.fixedBottom {
  width: 100%;
  height: 128rpx;
  padding: 20rpx 30rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  box-shadow: 0 -2rpx 4rpx #e8e8e8;
}
.fixedBottom .button {
  border: 0;
  padding: 0;
  height: 88rpx;
  line-height: 88rpx;
}
.modal__body {
  max-height: 900rpx;
  overflow-y: scroll;
}
.popup__body {
  max-height: 600rpx;
  overflow-y: scroll;
}
#error {
  min-height: 100vh;
  background: #fff;
  text-align: center;
}
#error .content {
  position: absolute;
  top: 40%;
  left: 50%;
  transform: translate(-50%, -50%);
}
#error image {
  display: inline-block;
  width: 160rpx;
  height: 160rpx;
  vertical-align: top;
}
#error .reloadLink {
  margin-top: 40rpx;
  font-size: 28rpx;
  text-decoration: underline;
}
