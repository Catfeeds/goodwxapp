<view class="timeList"><view class="timeList__left"><block wx:if="{{ index }}"><image class="icon" wx:if="{{ index === 1 }}" src="https://imgcdn.youhaodongxi.com/weapp/icon-first-a626108e34.png"></image><image class="icon" wx:elif="{{ index === 2 }}" src="https://imgcdn.youhaodongxi.com/weapp/icon-second-86fe078d2a.png"></image><image class="icon" wx:elif="{{ index === 3 }}" src="https://imgcdn.youhaodongxi.com/weapp/icon-third-15b0e662a0.png"></image><view class="numList" wx:elif="{{ index === 4 }}"><text class="num ui-fontSize20 ui-blackLighter">{{ index }}</text></view><view class="numList" wx:elif="{{ index === 5 }}"><text class="num ui-fontSize20 ui-blackLighter">{{ index }}</text></view></block></view><view class="timeList__middle"><image class="compic" src="{{ merchandiseImg }}"></image></view><view class="timeList__right"><view class="text ui-fontSize14">{{ merchandiseTitle }}</view><view class="howPeople ui-marginTop15"><view><image class="people" wx:for="{{ avatarsList }}" src="{{ item }}" wx:key="{{ key }}"></image></view><view><text class="ui-fontSize10 ui-marginLeft5 ui-blackLight">{{ buyerNum }}人购买</text></view></view></view></view>