/* pages/tixian-record/tixian-record.wxss */
.container{
  background-color: #f2f2f2;
  min-height: 1200rpx;
}
.section-ft{
  font-size: 24rpx;
  line-height: 76rpx;
  text-align: center;
  color: #878787;
}
.title{
  background-color: #ffffff;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 26rpx;
  text-align: center;
  display: flex;
  justify-content: space-around
}
.tixian-item{
  background-color: #ffffff;
  height: 80rpx;
  font-size: 24rpx;
  line-height: 80rpx;
  margin-top: 2rpx;
  text-align: center;
  display: flex;
  justify-content: space-around
}
.time,.title-item:nth-of-type(1){
  width: 240rpx;
}
.money,.title-item:nth-of-type(2){
  width: 180rpx;
}
.result,.title-item:nth-of-type(3){
  width: 80rpx;
}
.checkResult,.title-item:nth-of-type(4){
  width: 100rpx;
}
.checkResult{
  color: #268bf2
}