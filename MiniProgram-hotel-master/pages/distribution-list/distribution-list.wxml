<!--pages/distribution-list/distribution-list.wxml-->
<view class='container'>
  <view class='title-list'>
    <view class='title-item' wx:for='{{title_items}}' wx:key='code'>{{item}}</view>
  </view>
  <view class='distribution-list'>
    <view class='distribution-item' wx:for='{{distribution_items}}' wx:key='code'>
      <view class='memberName'>{{item.memberName}}</view>
      <view class='time'>{{item.time}}</view>
      <view class='money'>￥{{item.money}}</view>
      <view class='rebate'>￥{{item.rebate}}</view>
    </view>
  </view>

  <view class='section-ft'>没有更多啦</view>
</view>