<!--pages/myTeam/myTeam.wxml-->
<view class='container'>
  <view class='myTeam'>
    <view class='myTeam-item' wx:for='{{myTeam}}' wx:key='code'>
      <view class='memberInfo'>
        <image class='memberPic' src='{{item.memberPic}}'></image>
        <view>
          <view class='memberName'>{{item.memberName}}</view>
          <view class='memberLevel'>{{item.memberLevel}}</view>
        </view>
      </view>
      <view class='teamInfo'>
        <view class='money'>提成金额：￥{{item.money}}</view>
        <view class='joinTime'>入团时间：{{item.joinTime}}</view>
      </view>
    </view>
  </view>
  <view class='section-ft'>没有更多啦</view>
</view>