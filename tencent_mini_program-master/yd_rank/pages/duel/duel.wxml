<!--duel.wxml-->
<view class="container">
<image class="personal-bg" src="../img/bg@2x.png"></image>
  <view class="personal">

    <view class="logo">
      <image src="../img/logo_duel@2x.png"></image>
       <text> 双人对战</text>

    </view>

    <view class="duel">
        <view class="duel__column">
            <image class="userinfo-avatar duel__column-avatar" src="{{duelInfo[0].head_url_https}}" background-size="cover"></image>
            <view class="duel__column-score">{{duelInfo[0].steps}}</view>
            <view class="duel__column-result">{{duelInfo[0].result}}</view>
        </view>
        <view class="duel__pic">
            <image src="../img/pic_vs@2x.png"></image>
        </view>
        <view class="duel__column duel__column-right">
            <image class="userinfo-avatar duel__column-avatar" src="{{duelInfo[1].head_url_https}}" background-size="cover"></image> 
            <view class="duel__column-score">{{duelInfo[1].steps}}</view>
            <view class="duel__column-result">{{duelInfo[1].result}}</view>
        </view>
    </view>


    <button>转发到微信群，查看群内排行</button>
    <text class="personal-hint">为什么我没有数据</text>
  </view>
</view>



